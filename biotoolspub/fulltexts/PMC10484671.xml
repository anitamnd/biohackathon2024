\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
$12.5\%$\end{document}</tex-math></inline-formula> or <inline-formula><tex-math id="M0003" notation="LaTeX">\documentclass[12pt]{minimal}
\usepackage{amsmath}
