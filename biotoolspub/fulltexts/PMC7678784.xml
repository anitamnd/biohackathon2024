            <td valign="top" align="left" rowspan="1" colspan="1">113</td>
          </tr>
          <tr>
            <td valign="top" align="left" rowspan="1" colspan="1">Class C</td>
            <td valign="top" align="left" rowspan="1" colspan="1">Glutamate-like</td>
            <td valign="top" align="left" rowspan="1" colspan="1">112</td>
          </tr>
          <tr>
            <td valign="top" align="left" rowspan="1" colspan="1">Class D</td>
            <td valign="top" align="left" rowspan="1" colspan="1">Fungal pheromone<xref ref-type="table-fn" rid="T0001-fn1"><sup>a</sup></xref></td>
            <td valign="top" align="left" rowspan="1" colspan="1">13</td>
          </tr>
          <tr>
            <td valign="top" align="left" rowspan="1" colspan="1">Class E</td>
            <td valign="top" align="left" rowspan="1" colspan="1">cAMP receptor<xref ref-type="table-fn" rid="T0001-fn1"><sup>a</sup></xref></td>
