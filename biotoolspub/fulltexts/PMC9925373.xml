              <td align="left" rowspan="1" colspan="1">−10</td>
              <td align="left" rowspan="1" colspan="1">TLE3</td>
              <td align="left" rowspan="1" colspan="1">T334</td>
              <td align="left" rowspan="1" colspan="1">Transducin‐like enhancer protein 3</td>
              <td align="char" rowspan="1" colspan="1">
                <bold>−4.46</bold>
