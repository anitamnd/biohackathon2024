            </mml:mtable>
          </mml:math>
        </disp-formula>
        <disp-formula id="E6">
          <label>(6)</label>
          <mml:math id="M6">
            <mml:mtable class="eqnarray" columnalign="left">
              <mml:mtr>
                <mml:mtd>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>x</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mi>r</mml:mi>
