}{}\begin{eqnarray*} \displaystyle \left\{\extrarowheight =1.2pc \begin{array}{@{}l@{}} \displaystyle \text{Sn}=\frac{\text{TP}}{\text{TP}+\text{FN}}\\ \displaystyle \text{Sp}=\frac{\text{TN}}{\text{TN}+\text{FP}}\\ \displaystyle \text{Acc}=\frac{\text{TP + TN}}{\text{TP}+\text{TN}+\text{FP}+\text{FN}}\\ \displaystyle \text{MCC}=\frac{(\text{TP}\times \text{TN})-(\text{FP}\times \text{FN})}{\sqrt{\text{(TP + FP)(TP + FN)(TN + FP)(TN + FN)}}} \end{array}\right.&amp;&amp;\displaystyle \end{eqnarray*}\end{document}</tex-math><mml:math id="mml-eqn-18"><mml:mstyle displaystyle="true"><mml:mfenced separators="" open="{" close=""><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="left"><mml:mtext>Sn</mml:mtext><mml:mo>=</mml:mo><mml:mfrac><mml:mrow><mml:mtext>TP</mml:mtext></mml:mrow><mml:mrow><mml:mtext>TP</mml:mtext><mml:mo>+</mml:mo><mml:mtext>FN</mml:mtext></mml:mrow></mml:mfrac></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="left"><mml:mtext>Sp</mml:mtext><mml:mo>=</mml:mo><mml:mfrac><mml:mrow><mml:mtext>TN</mml:mtext></mml:mrow><mml:mrow><mml:mtext>TN</mml:mtext><mml:mo>+</mml:mo><mml:mtext>FP</mml:mtext></mml:mrow></mml:mfrac></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="left"><mml:mtext>Acc</mml:mtext><mml:mo>=</mml:mo><mml:mfrac><mml:mrow><mml:mtext>TP + TN</mml:mtext></mml:mrow><mml:mrow><mml:mtext>TP</mml:mtext><mml:mo>+</mml:mo><mml:mtext>TN</mml:mtext><mml:mo>+</mml:mo><mml:mtext>FP</mml:mtext><mml:mo>+</mml:mo><mml:mtext>FN</mml:mtext></mml:mrow></mml:mfrac></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="left"><mml:mtext>MCC</mml:mtext><mml:mo>=</mml:mo><mml:mfrac><mml:mrow><mml:mo>(</mml:mo><mml:mtext>TP</mml:mtext><mml:mo>×</mml:mo><mml:mtext>TN</mml:mtext><mml:mo>)</mml:mo><mml:mo>−</mml:mo><mml:mo>(</mml:mo><mml:mtext>FP</mml:mtext><mml:mo>×</mml:mo><mml:mtext>FN</mml:mtext><mml:mo>)</mml:mo></mml:mrow><mml:mrow><mml:msqrt><mml:mrow><mml:mtext>(TP + FP)(TP + FN)(TN + FP)(TN + FN)</mml:mtext></mml:mrow></mml:msqrt></mml:mrow></mml:mfrac></mml: