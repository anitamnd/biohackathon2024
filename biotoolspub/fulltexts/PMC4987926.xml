              <td align="left" rowspan="1" colspan="1"/>
              <td align="left" rowspan="1" colspan="1">Hairpin</td>
              <td align="center" rowspan="1" colspan="1"/>
              <td align="left" rowspan="1" colspan="1">&lt;3-mer</td>
            </tr>
            <tr>
              <td align="left" rowspan="1" colspan="1">Primer pair</td>
              <td align="left" rowspan="1" colspan="1">length difference</td>
              <td align="left" rowspan="1" colspan="1">-</td>
              <td align="left" rowspan="1" colspan="1">≤5-mer</td>
            </tr>
