            <tr>
              <td valign="top" rowspan="1" colspan="1">Control</td>
              <td valign="top" rowspan="1" colspan="1">1</td>
              <td valign="top" rowspan="1" colspan="1">3A3</td>
            </tr>
            <tr>
              <td valign="top" rowspan="1" colspan="1">Control</td>
