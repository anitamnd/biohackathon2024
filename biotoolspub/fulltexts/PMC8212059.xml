                        </mml:mtd>
                      </mml:mtr>
                      <mml:mtr>
                        <mml:mtd>
                          <mml:mi>Î²</mml:mi>
                        </mml:mtd>
                        <mml:mtd>
                          <mml:mn>1</mml:mn>
                          <mml:mo>-</mml:mo>
                          <mml:mi>Î²</mml:mi>
