                      </mml:mrow>
                    </mml:munder>
                    <mml:mi>P</mml:mi>
                    <mml:mo stretchy="true">(</mml:mo>
                    <mml:mi>t</mml:mi>
                    <mml:mo stretchy="true">)</mml:mo>
                  </mml:mrow>
                </mml:mfenced>
              </mml:mrow>
            </mml:mfenced>
            <mml:mtext>.</mml:mtext>
          </mml:mrow>
