            <mml:mi>S</mml:mi>
            <mml:munderover accentunder="true">
              <mml:mo>⇀</mml:mo>
              <mml:munder>
                <mml:mo>↽</mml:mo>
                <mml:msub>
                  <mml:mi>k</mml:mi>
