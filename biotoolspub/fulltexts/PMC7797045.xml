\usepackage{wasysym} 
\usepackage{amsfonts} 
\usepackage{amssymb} 
\usepackage{amsbsy}
\usepackage{upgreek}
\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$$\begin{equation*}{\rm{p }} = \frac{{\# ({{\rm{N}}_{{\rm{rand}}}} &gt;{{\rm{N}}_{{\rm{real}}}})}}{{\rm{n}}}\ \end{equation*}$$\end{document}</tex-math></disp-formula>Where <inline-formula><tex-math id="M24">\documentclass[12pt]{minimal}
\usepackage{amsmath}
