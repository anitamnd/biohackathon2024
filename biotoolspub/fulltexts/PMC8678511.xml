              <mml:mi>C</mml:mi>
            </mml:mrow>
            <mml:mo>=</mml:mo>
            <mml:mfrac>
              <mml:mrow>
                <mml:mrow>
                  <mml:mi>T</mml:mi>
                  <mml:mo>⁢</mml:mo>
                  <mml:mi>P</mml:mi>
