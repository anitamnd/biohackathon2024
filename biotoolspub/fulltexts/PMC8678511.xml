                </mml:mrow>
              </mml:mrow>
              <mml:mrow>
                <mml:mrow>
                  <mml:mi>T</mml:mi>
                  <mml:mo>⁢</mml:mo>
                  <mml:mi>P</mml:mi>
                </mml:mrow>
                <mml:mo>+</mml:mo>
                <mml:mrow>
                  <mml:mi>T</mml:mi>
                  <mml:mo>⁢</mml:mo>
                  <mml:mi>N</mml:mi>
                </mml:mrow>
                <mml:mo>+</mml:mo>
                <mml:mrow>
                  <mml:mi>F</mml:mi>
