                </mml:mrow>
                <mml:mo>+</mml:mo>
                <mml:mrow>
                  <mml:mi>F</mml:mi>
                  <mml:mo>⁢</mml:mo>
                  <mml:mi>N</mml:mi>
                </mml:mrow>
