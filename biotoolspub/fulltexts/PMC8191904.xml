            <mml:mfrac>
              <mml:mrow>
                <mml:mi>d</mml:mi>
                <mml:msub>
                  <mml:mrow>
                    <mml:mi>M</mml:mi>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mi>m</mml:mi>
                  </mml:mrow>
                </mml:msub>
              </mml:mrow>
              <mml:mrow>
                <mml:mi>d</mml:mi>
                <mml:mi>t</mml:mi>
              </mml:mrow>
            </mml:mfrac>
            <mml:mo>=</mml:mo>
            <mml:msub>
              <mml:mrow>
                <mml:mi>F</mml:mi>
              </mml:mrow>
              <mml:mrow>
                <mml:mi>i</mml:mi>
                <mml:mi>n</mml:mi>
              </mml:mrow>
            </mml:msub>
          </mml:math>
        </alternatives>
        <label>(8)</label>
      </disp-formula>
      <disp-formula id="pone.0252881.e044">
