                    <mml:mi>O</mml:mi>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mn>2</mml:mn>
                  </mml:mrow>
                </mml:msub>
                <mml:mo>,</mml:mo>
                <mml:mi>s</mml:mi>
                <mml:mi>r</mml:mi>
              </mml:mrow>
            </mml:msub>
            <mml:mo>∙</mml:mo>
            <mml:msub>
              <mml:mrow>
                <mml:mi>r</mml:mi>
