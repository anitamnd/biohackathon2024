        <label>(10)</label>
      </disp-formula>
      <disp-formula id="pone.0252881.e047">
        <alternatives>
          <graphic xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="pone.0252881.e047.jpg" id="pone.0252881.e047g" position="anchor"/>
