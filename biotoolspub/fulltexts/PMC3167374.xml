              <col align="center" char="."/>
            </colgroup>
            <thead>
              <tr>
                <th rowspan="2" style="border:none;" align="center"> </th>
                <th style="border:none;" align="center" char="."> </th>
                <th style="border:none;" align="center" char="."> </th>
                <th colspan="4" align="center" char=".">protein reporting method<hr/></th>
              </tr>
              <tr>
                <th align="center" char=".">MS/MS spectra</th>
