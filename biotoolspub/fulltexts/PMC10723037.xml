            <tr>
              <td rowspan="1" colspan="1">Pep_Test_639</td>
              <td rowspan="1" colspan="1">639</td>
              <td rowspan="1" colspan="1">8490</td>
              <td rowspan="1" colspan="1">141 840</td>
              <td rowspan="1" colspan="1">0.060</td>
            </tr>
            <tr>
              <td rowspan="3" colspan="1">Protein</td>
              <td rowspan="1" colspan="1">Pro_Train_335</td>
              <td rowspan="1" colspan="1">335</td>
              <td rowspan="1" colspan="1">10 374</td>
              <td rowspan="1" colspan="1">55 992</td>
              <td rowspan="1" colspan="1">0.185</td>
            </tr>
            <tr>
              <td rowspan="1" colspan="1">Pro_Test_60</td>
              <td rowspan="1" colspan="1">60</td>
              <td rowspan="1" colspan="1">2075</td>
              <td rowspan="1" colspan="1">11 069</td>
              <td rowspan="1" colspan="1">0.187</td>
            </tr>
            <tr>
              <td rowspan="1" colspan="1">Pro_Test_315</td>
              <td rowspan="1" colspan="1">315</td>
              <td rowspan="1" colspan="1">9355</td>
              <td rowspan="1" colspan="1">55 976</td>
              <td rowspan="1" colspan="1">0.167</td>
