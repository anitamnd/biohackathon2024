                      </mml:mfrac>
                    </mml:mrow>
                    <mml:mo>,</mml:mo>
                    <mml:mo> </mml:mo>
                    <mml:mrow>
                      <mml:msub>
                        <mml:mrow>
                          <mml:mo> </mml:mo>
                          <mml:mi>n</mml:mi>
