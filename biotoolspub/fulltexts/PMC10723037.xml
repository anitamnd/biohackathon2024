                        </mml:mrow>
                      </mml:msub>
                    </mml:mrow>
                    <mml:mo>=</mml:mo>
                    <mml:mrow>
                      <mml:mfrac>
                        <mml:mrow>
                          <mml:mi>x</mml:mi>
                          <mml:mo>-</mml:mo>
                          <mml:mrow>
                            <mml:msub>
