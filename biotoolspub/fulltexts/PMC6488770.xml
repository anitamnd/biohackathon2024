}{}
\begin{equation*}
P(D_l^i,D^j_l\mid X_l = m,f_l^A) \\
\quad = \sum _{G_l^i,G_l^j \in \lbrace 0,1,2\rbrace ^2}P(D_l^i\mid G_l^i)P(D_l^j\mid G_l^j)P(G_l^i,G_l^j\mid f_l^A,X_l=m),
\end{equation*}
\end{document}</tex-math></disp-formula></p>
    <p>where <inline-formula><tex-math id="M14">\documentclass[12pt]{minimal}
