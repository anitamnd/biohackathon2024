                  </mml:msub>
                  <mml:mo>(</mml:mo>
                  <mml:mi>j</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mi>k</mml:mi>
                  <mml:mo>−</mml:mo>
                  <mml:mn>1</mml:mn>
                  <mml:mo>)</mml:mo>
                  <mml:mo>·</mml:mo>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>α</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext>Stem</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
                  <mml:mo>(</mml:mo>
                  <mml:mi>p</mml:mi>
                  <mml:mo>−</mml:mo>
                  <mml:mn>1</mml:mn>
                  <mml:mo>,</mml:mo>
                  <mml:mi>q</mml:mi>
                  <mml:mo>)</mml:mo>
                  <mml:mo>·</mml:mo>
                  <mml:mi>t</mml:mi>
                  <mml:mo>(</mml:mo>
                  <mml:mtext>StemEnd</mml:mtext>
                  <mml:mo>→</mml:mo>
                  <mml:mo>(</mml:mo>
                  <mml:mtext>Interior</mml:mtext>
                  <mml:mo>)</mml:mo>
                  <mml:mo>→</mml:mo>
                  <mml:mtext>Stem</mml:mtext>
                  <mml:mo>)</mml:mo>
                </mml:mtd>
                <mml:mtd>
                  <mml:mtext/>
                </mml:mtd>
              </mml:mtr>
              <mml:mtr columnalign="right">
                <mml:mtd class="eqnarray-1">
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>w</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext>InternalRight</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
                  <mml:mo>(</mml:mo>
                  <mml:mi>i</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mi>I</mml:mi>
                  <mml:mo>)</mml:mo>
                </mml:mtd>
                <mml:mtd class="eqnarray-2">
                  <mml:mo>=</mml:mo>
                </mml:mtd>
                <mml:mtd class="eqnarray-3" columnalign="left">
                  <mml:munderover>
                    <mml:mrow>
                      <mml:mo mathsize="big">∑</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mi>j</mml:mi>
                      <mml:mo>=</mml:mo>
                      <mml:mtext>max</mml:mtext>
                      <mml:mo>(</mml:mo>
                      <mml:mn>1</mml:mn>
                      <mml:mo>,</mml:mo>
                      <mml:mi>i</mml:mi>
                      <mml:mo>−</mml:mo>
                      <mml:mi>W</mml:mi>
                      <mml:mo>)</mml:mo>
