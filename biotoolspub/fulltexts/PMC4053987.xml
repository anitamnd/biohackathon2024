                      <mml:mo>(</mml:mo>
                      <mml:mi>j</mml:mi>
                      <mml:mo>+</mml:mo>
                      <mml:mi>C</mml:mi>
                      <mml:mo>+</mml:mo>
                      <mml:mn>1</mml:mn>
                      <mml:mo>,</mml:mo>
                      <mml:mi>i</mml:mi>
                      <mml:mo>−</mml:mo>
                      <mml:mn>1</mml:mn>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                  </mml:munderover>
                  <mml:munderover>
                    <mml:mrow>
                      <mml:mo mathsize="big">∑</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mi>q</mml:mi>
                      <mml:mo>=</mml:mo>
                      <mml:mtext>max</mml:mtext>
                      <mml:mo>(</mml:mo>
                      <mml:mi>p</mml:mi>
                      <mml:mo>+</mml:mo>
                      <mml:mn>4</mml:mn>
                      <mml:mo>,</mml:mo>
                      <mml:mi>k</mml:mi>
                      <mml:mo>−</mml:mo>
                      <mml:mi>C</mml:mi>
                      <mml:mo>−</mml:mo>
                      <mml:mi>p</mml:mi>
                      <mml:mo>+</mml:mo>
                      <mml:mi>j</mml:mi>
                      <mml:mo>−</mml:mo>
                      <mml:mn>1</mml:mn>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mi>i</mml:mi>
                    </mml:mrow>
                  </mml:munderover>
                </mml:mtd>
                <mml:mtd>
                  <mml:mtext/>
                </mml:mtd>
              </mml:mtr>
              <mml:mtr columnalign="right">
                <mml:mtd class="eqnarray-1"/>
                <mml:mtd class="eqnarray-2"/>
                <mml:mtd class="eqnarray-3" columnalign="left">
                  <mml:mspace width="1em"/>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>β</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext>StemEnd</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
                  <mml:mo>(</mml:mo>
                  <mml:mi>j</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mi>k</mml:mi>
                  <mml:mo>−</mml:mo>
                  <mml:mn>1</mml:mn>
                  <mml:mo>)</mml:mo>
                  <mml:mo>·</mml:mo>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>α</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext>Stem</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
