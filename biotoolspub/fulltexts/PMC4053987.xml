                      <mml:mo>=</mml:mo>
                      <mml:mi>i</mml:mi>
                      <mml:mo>+</mml:mo>
                      <mml:mn>1</mml:mn>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext>min</mml:mtext>
                      <mml:mo>(</mml:mo>
                      <mml:mi>n</mml:mi>
                      <mml:mo>,</mml:mo>
                      <mml:mi>j</mml:mi>
                      <mml:mo>+</mml:mo>
                      <mml:mi>W</mml:mi>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                  </mml:munderover>
                  <mml:munderover>
