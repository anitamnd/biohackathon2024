\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}\begin{equation*} F\left({R}_1,{R}_2\right)=1-\frac{\sum_{j=1}^K\mathrm{score}\left({R}_1(j),{R}_2(j)\right)}{K} \end{equation*}\end{document}</tex-math></disp-formula>where <inline-formula><tex-math notation="LaTeX" id="ineq03">\documentclass[12pt]{minimal}
