				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ C(s,w,1:R) $$\end{document}</tex-math>
              <mml:math id="M238">
                <mml:mrow>
                  <mml:mi>C</mml:mi>
                  <mml:mo stretchy="false">(</mml:mo>
                  <mml:mi>s</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mi>w</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mn>1</mml:mn>
                  <mml:mo>:</mml:mo>
                  <mml:mi>R</mml:mi>
                  <mml:mo stretchy="false">)</mml:mo>
                </mml:mrow>
