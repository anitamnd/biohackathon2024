				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ C\left( {s - 1, 1:w, 1} \right) $$\end{document}</tex-math>
              <mml:math id="M196">
                <mml:mrow>
                  <mml:mi>C</mml:mi>
                  <mml:mfenced close=")" open="(" separators="">
                    <mml:mrow>
                      <mml:mi>s</mml:mi>
                      <mml:mo>-</mml:mo>
                      <mml:mn>1</mml:mn>
