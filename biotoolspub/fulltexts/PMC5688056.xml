              <mml:math id="M180">
                <mml:mrow>
                  <mml:mi>C</mml:mi>
                  <mml:mo stretchy="false">(</mml:mo>
                  <mml:mn>0</mml:mn>
                  <mml:mo>,</mml:mo>
                  <mml:mi>w</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mi>r</mml:mi>
                  <mml:mo stretchy="false">)</mml:mo>
                </mml:mrow>
              </mml:math>
