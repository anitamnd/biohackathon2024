          <italic>, we can infer the value of</italic>
          <inline-formula id="IEq93">
            <alternatives>
              <tex-math id="M197">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ C(s, w, 1) $$\end{document}</tex-math>
              <mml:math id="M198">
                <mml:mrow>
                  <mml:mi>C</mml:mi>
                  <mml:mo stretchy="false">(</mml:mo>
                  <mml:mi>s</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mi>w</mml:mi>
                  <mml:mo>,</mml:mo>
                  <mml:mn>1</mml:mn>
