                                  <mml:mrow>
                                    <mml:mo>-</mml:mo>
                                    <mml:mrow>
                                      <mml:mi>l</mml:mi>
                                      <mml:mi>g</mml:mi>
                                      <mml:mrow>
                                        <mml:mo stretchy="false">(</mml:mo>
                                        <mml:mrow>
                                          <mml:mi>E</mml:mi>
                                          <mml:mi>v</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>l</mml:mi>
                                          <mml:mi>u</mml:mi>
                                          <mml:mi>e</mml:mi>
                                        </mml:mrow>
                                        <mml:mo stretchy="false">)</mml:mo>
                                      </mml:mrow>
