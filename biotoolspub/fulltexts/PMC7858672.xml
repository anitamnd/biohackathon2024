                </mml:mrow>
              </mml:mtd>
            </mml:mtr>
            <mml:mtr>
              <mml:mtd columnalign="center">
                <mml:mrow>
                  <mml:mrow>
                    <mml:mo>×</mml:mo>
                    <mml:mrow>
                      <mml:mrow>
                        <mml:mi>w</mml:mi>
                        <mml:mn>2</mml:mn>
                      </mml:mrow>
                      <mml:mo>+</mml:mo>
                      <mml:mrow>
                        <mml:mrow>
                          <mml:mstyle displaystyle="true">
                            <mml:mfrac>
                              <mml:mrow>
                                <mml:mi>l</mml:mi>
                                <mml:mi>o</mml:mi>
                                <mml:msub>
                                  <mml:mi>g</mml:mi>
