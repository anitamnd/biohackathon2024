				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$SCORE\;=\;Coverage-\;\lambda_1\;\times\;W_1\;-\lambda_2\;\times\;\left(\lambda_3\;\times\;W_2\;+\;\lambda_4\;\times\;W_3\;+\;\lambda_5\;\times W_4\;+\;\lambda_6\;\times W_5\right)$$\end{document}</tex-math>
            <mml:math id="M2" display="block">
