                <mml:mi>C</mml:mi>
                <mml:mi>o</mml:mi>
                <mml:mi>v</mml:mi>
                <mml:mi>e</mml:mi>
                <mml:mi>r</mml:mi>
                <mml:mi>a</mml:mi>
                <mml:mi>g</mml:mi>
                <mml:mi>e</mml:mi>
                <mml:mo>-</mml:mo>
                <mml:mspace width="0.277778em"/>
                <mml:msub>
                  <mml:mi>λ</mml:mi>
                  <mml:mn>1</mml:mn>
                </mml:msub>
                <mml:mspace width="0.277778em"/>
                <mml:mo>×</mml:mo>
                <mml:mspace width="0.277778em"/>
                <mml:msub>
                  <mml:mi>W</mml:mi>
                  <mml:mn>1</mml:mn>
                </mml:msub>
                <mml:mspace width="0.277778em"/>
                <mml:mo>-</mml:mo>
                <mml:msub>
                  <mml:mi>λ</mml:mi>
                  <mml:mn>2</mml:mn>
                </mml:msub>
                <mml:mspace width="0.277778em"/>
                <mml:mo>×</mml:mo>
                <mml:mspace width="0.277778em"/>
                <mml:mfenced close=")" open="(">
                  <mml:msub>
                    <mml:mi>λ</mml:mi>
                    <mml:mn>3</mml:mn>
                  </mml:msub>
                  <mml:mspace width="0.277778em"/>
                  <mml:mo>×</mml:mo>
                  <mml:mspace width="0.277778em"/>
                  <mml:msub>
                    <mml:mi>W</mml:mi>
                    <mml:mn>2</mml:mn>
                  </mml:msub>
                  <mml:mspace width="0.277778em"/>
                  <mml:mo>+</mml:mo>
                  <mml:mspace width="0.277778em"/>
                  <mml:msub>
                    <mml:mi>λ</mml:mi>
                    <mml:mn>4</mml:mn>
                  </mml:msub>
                  <mml:mspace width="0.277778em"/>
