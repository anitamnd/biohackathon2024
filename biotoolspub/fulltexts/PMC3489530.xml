                      <mml:mi>x</mml:mi>
                    </mml:mrow>
                    <mml:mo class="MathClass-op"> Â¯</mml:mo>
                  </mml:mover>
                </mml:mrow>
                <mml:mrow>
                  <mml:mn>1</mml:mn>
                </mml:mrow>
              </mml:msub>
              <mml:mo class="MathClass-punc">,</mml:mo>
              <mml:msub>
                <mml:mrow>
                  <mml:mover accent="true">
                    <mml:mrow>
                      <mml:mi>x</mml:mi>
                    </mml:mrow>
                    <mml:mo class="MathClass-op"> Â¯</mml:mo>
