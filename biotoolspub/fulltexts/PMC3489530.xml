                <mml:mrow>
                  <mml:mi>È³</mml:mi>
                </mml:mrow>
                <mml:mrow>
                  <mml:mn>1</mml:mn>
                </mml:mrow>
              </mml:msub>
              <mml:mo class="MathClass-punc">,</mml:mo>
              <mml:mspace class="thinspace" width="0.3em"/>
              <mml:mstyle class="text">
                <mml:mtext class="textsf" mathvariant="sans-serif">and</mml:mtext>
              </mml:mstyle>
              <mml:mspace class="thinspace" width="0.3em"/>
              <mml:msub>
                <mml:mrow>
