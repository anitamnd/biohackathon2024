              <td align="left" valign="top" rowspan="1" colspan="1">5</td>
            </tr>
            <tr>
              <td align="left" valign="top" rowspan="1" colspan="1">CHEMBL1214334</td>
              <td align="left" valign="top" rowspan="1" colspan="1">5.900<xref rid="pro3732-bib-0080" ref-type="ref">80</xref>
</td>
              <td align="left" valign="top" rowspan="1" colspan="1">0.75</td>
              <td align="left" valign="top" rowspan="1" colspan="1">−6.57</td>
              <td align="left" valign="top" rowspan="1" colspan="1">2.26</td>
              <td align="left" valign="top" rowspan="1" colspan="1">8,452</td>
              <td align="left" valign="top" rowspan="1" colspan="1">6</td>
            </tr>
