                        </mml:mrow>
                        <mml:mo>¯</mml:mo>
                      </mml:mover>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mi>h</mml:mi>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                      <mml:mo>=</mml:mo>
                      <mml:mfrac>
                        <mml:mrow>
                          <mml:mo>∑</mml:mo>
                          <mml:mrow>
                            <mml:mi>P</mml:mi>
