              <mml:mi>e</mml:mi>
              <mml:mi>l</mml:mi>
              <mml:mi>i</mml:mi>
              <mml:mi>c</mml:mi>
              <mml:mi>a</mml:mi>
              <mml:mi>l</mml:mi>
              <mml:mi>i</mml:mi>
              <mml:mi>t</mml:mi>
              <mml:mi>y</mml:mi>
              <mml:mo>=</mml:mo>
              <mml:mfrac>
                <mml:mn>1</mml:mn>
                <mml:mrow>
                  <mml:msup>
                    <mml:mrow>
