                      </mml:mrow>
                    </mml:msub>
                    <mml:mo>+</mml:mo>
                    <mml:mi>ϕ</mml:mi>
                    <mml:mi>S</mml:mi>
                    <mml:mi>i</mml:mi>
                    <mml:msub>
                      <mml:mi>m</mml:mi>
