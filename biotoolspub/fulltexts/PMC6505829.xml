            <mml:mi mathvariant="normal">x</mml:mi>
          </mml:mrow>
          <mml:mo>)</mml:mo>
          <mml:mo>=</mml:mo>
          <mml:mfrac>
            <mml:mrow>
              <mml:mn>2</mml:mn>
            </mml:mrow>
            <mml:mrow>
              <mml:mn>1</mml:mn>
              <mml:mo>+</mml:mo>
              <mml:msup>
                <mml:mrow>
                  <mml:mi mathvariant="normal">e</mml:mi>
                </mml:mrow>
