              <td rowspan="1" colspan="1">Freecontact</td>
              <td rowspan="1" colspan="1">25.7</td>
              <td rowspan="1" colspan="1">17.9</td>
              <td rowspan="1" colspan="1">12.9</td>
              <td rowspan="1" colspan="1">32.0</td>
              <td rowspan="1" colspan="1">22.6</td>
              <td rowspan="1" colspan="1">15.6</td>
              <td rowspan="1" colspan="1">36.2</td>
