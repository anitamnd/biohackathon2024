              <mml:msup>
                <mml:mi>X</mml:mi>
                <mml:mrow>
                  <mml:mi>t</mml:mi>
                  <mml:mi>r</mml:mi>
                  <mml:mi>a</mml:mi>
                  <mml:mi>i</mml:mi>
                  <mml:mi>n</mml:mi>
                </mml:mrow>
              </mml:msup>
              <mml:mo>∙</mml:mo>
              <mml:mi>β</mml:mi>
              <mml:mo>+</mml:mo>
