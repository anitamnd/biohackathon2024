              <mml:mo>∙</mml:mo>
              <mml:msup>
                <mml:mi>X</mml:mi>
                <mml:mrow>
                  <mml:mi>t</mml:mi>
                  <mml:mi>r</mml:mi>
                  <mml:mi>a</mml:mi>
                  <mml:mi>i</mml:mi>
                  <mml:mi>n</mml:mi>
                </mml:mrow>
              </mml:msup>
              <mml:mo>∙</mml:mo>
              <mml:mi>β</mml:mi>
              <mml:mo>−</mml:mo>
              <mml:mi>M</mml:mi>
              <mml:mo>∙</mml:mo>
              <mml:mo stretchy="false">(</mml:mo>
              <mml:mrow>
                <mml:mn>1</mml:mn>
                <mml:mo>−</mml:mo>
                <mml:mi>z</mml:mi>
