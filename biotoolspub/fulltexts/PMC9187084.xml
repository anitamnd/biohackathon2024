      <disp-formula id="pcbi.1010180.e022">
        <alternatives>
          <graphic xlink:href="pcbi.1010180.e022.jpg" id="pcbi.1010180.e022g" position="anchor"/>
          <mml:math id="M22" display="block" overflow="scroll">
            <mml:mrow>
              <mml:mo>−</mml:mo>
              <mml:mi>M</mml:mi>
              <mml:mo>∙</mml:mo>
              <mml:mi>z</mml:mi>
              <mml:mo>≤</mml:mo>
              <mml:mi>β</mml:mi>
              <mml:mo>≤</mml:mo>
              <mml:mi>M</mml:mi>
              <mml:mo>∙</mml:mo>
              <mml:mi>z</mml:mi>
