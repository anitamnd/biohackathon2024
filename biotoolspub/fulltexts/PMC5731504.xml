                    <mml:mtext/>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd class="eqnarray-1"/>
                  <mml:mtd class="eqnarray-2">
                    <mml:mo>=</mml:mo>
                  </mml:mtd>
                  <mml:mtd class="eqnarray-3">
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mo>|</mml:mo>
                        <mml:mi>K</mml:mi>
                        <mml:mo>|</mml:mo>
                      </mml:mrow>
                    </mml:mfrac>
                    <mml:mo>·</mml:mo>
                    <mml:mfenced close="" open="(" separators="">
                      <mml:mrow>
                        <mml:mo>|</mml:mo>
                        <mml:msub>
                          <mml:mrow>
                            <mml:mi>N</mml:mi>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mi mathvariant="italic">kc</mml:mi>
                          </mml:mrow>
                        </mml:msub>
                        <mml:mfenced close=")" open="(" separators="">
                          <mml:mrow>
                            <mml:mi>X</mml:mi>
                            <mml:msub>
                              <mml:mrow>
                                <mml:mo>|</mml:mo>
                              </mml:mrow>
                              <mml:mrow>
                                <mml:mn>2</mml:mn>
                              </mml:mrow>
                            </mml:msub>
                            <mml:mo>,</mml:mo>
                            <mml:mi>K</mml:mi>
                            <mml:msub>
