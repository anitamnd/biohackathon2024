                        <mml:mo>|</mml:mo>
                      </mml:mrow>
                    </mml:mfrac>
                    <mml:mo>·</mml:mo>
                    <mml:mfenced close="" open="(" separators="">
                      <mml:mrow>
                        <mml:mo>|</mml:mo>
                        <mml:msub>
                          <mml:mrow>
                            <mml:mi>N</mml:mi>
