                  <mml:mtd class="eqnarray-2"/>
                  <mml:mtd class="eqnarray-3">
                    <mml:mi mathvariant="italic">precision</mml:mi>
                    <mml:mo>(</mml:mo>
                    <mml:mi>X</mml:mi>
                    <mml:mo>,</mml:mo>
                    <mml:mi>K</mml:mi>
                    <mml:mo>)</mml:mo>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext/>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd class="eqnarray-1"/>
                  <mml:mtd class="eqnarray-2">
                    <mml:mo>=</mml:mo>
                  </mml:mtd>
                  <mml:mtd class="eqnarray-3">
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mo>|</mml:mo>
