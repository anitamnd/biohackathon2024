                    <mml:mi>u</mml:mi>
                    <mml:mo>,</mml:mo>
                    <mml:mi>t</mml:mi>
                    <mml:mo stretchy="false">)</mml:mo>
                  </mml:mrow>
                </mml:msub>
                <mml:mo>=</mml:mo>
                <mml:munderover>
                  <mml:mo movablelimits="false">∑</mml:mo>
                  <mml:mrow>
                    <mml:mi>n</mml:mi>
                    <mml:mo>=</mml:mo>
                    <mml:mn>1</mml:mn>
                  </mml:mrow>
                  <mml:mi>M</mml:mi>
                </mml:munderover>
                <mml:mfrac>
                  <mml:mrow>
                    <mml:msup>
