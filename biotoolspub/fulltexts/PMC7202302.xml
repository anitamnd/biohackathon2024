                        <mml:mi>t</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                      <mml:mi>v</mml:mi>
                    </mml:msubsup>
                    <mml:mrow>
                      <mml:mo stretchy="false">(</mml:mo>
