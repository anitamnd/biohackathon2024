                        <mml:mi>σ</mml:mi>
                        <mml:mi>h</mml:mi>
                      </mml:msup>
                      <mml:mo>+</mml:mo>
                      <mml:msup>
                        <mml:mi>μ</mml:mi>
                        <mml:mi>h</mml:mi>
                      </mml:msup>
                      <mml:mo stretchy="false">)</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mo stretchy="false">(</mml:mo>
                      <mml:msup>
                        <mml:mi>γ</mml:mi>
