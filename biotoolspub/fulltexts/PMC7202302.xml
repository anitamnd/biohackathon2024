                        </mml:mrow>
                        <mml:mi>v</mml:mi>
                      </mml:msubsup>
                      <mml:mo>+</mml:mo>
                      <mml:msubsup>
                        <mml:mrow>
                          <mml:mi>μ</mml:mi>
                        </mml:mrow>
                        <mml:mrow>
                          <mml:mo stretchy="false">(</mml:mo>
                          <mml:mi>u</mml:mi>
                          <mml:mo>,</mml:mo>
                          <mml:mi>t</mml:mi>
                          <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                        <mml:mi>v</mml:mi>
