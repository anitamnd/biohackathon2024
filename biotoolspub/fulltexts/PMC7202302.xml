                        <mml:msubsup>
                          <mml:mi>a</mml:mi>
                          <mml:mrow>
                            <mml:mo stretchy="false">(</mml:mo>
                            <mml:mi>u</mml:mi>
                            <mml:mo stretchy="false">)</mml:mo>
                          </mml:mrow>
                          <mml:mi>v</mml:mi>
                        </mml:msubsup>
                      </mml:mrow>
                      <mml:mn>2</mml:mn>
                    </mml:msup>
                    <mml:msubsup>
                      <mml:mrow>
                        <mml:mi>ϕ</mml:mi>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>t</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mi>v</mml:mi>
                        <mml:mo stretchy="false">→</mml:mo>
                        <mml:mi>h</mml:mi>
                      </mml:mrow>
                    </mml:msubsup>
                    <mml:msup>
                      <mml:mi>ϕ</mml:mi>
                      <mml:mrow>
                        <mml:mi>h</mml:mi>
                        <mml:mo stretchy="false">→</mml:mo>
                        <mml:mi>v</mml:mi>
                      </mml:mrow>
                    </mml:msup>
                    <mml:msubsup>
                      <mml:mrow>
                        <mml:mi>γ</mml:mi>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>t</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                      <mml:mi>v</mml:mi>
                    </mml:msubsup>
