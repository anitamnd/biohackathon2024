          </tr>
          <tr>
            <td align="left" valign="bottom">20<hr/></td>
            <td align="left" valign="bottom">GSE3678<hr/></td>
            <td align="left" valign="bottom"> <hr/></td>
            <td align="left" valign="bottom">-<hr/></td>
            <td align="left" valign="bottom">Thyroid Cancer<hr/></td>
            <td align="left" valign="bottom">hsa05216<hr/></td>
            <td align="left" valign="bottom">Thyroid<hr/></td>
          </tr>
          <tr>
            <td align="left" valign="bottom">21<hr/></td>
            <td align="left" valign="bottom">GSE9476<hr/></td>
            <td align="left" valign="bottom">17910043<hr/></td>
            <td align="left" valign="bottom">[<xref ref-type="bibr" rid="B34">34</xref>]<hr/></td>
            <td align="left" valign="bottom">Acute myeloid leukemia<hr/></td>
            <td align="left" valign="bottom">hsa05221<hr/></td>
            <td align="left" valign="bottom">Blood, Bone marrow<hr/></td>
          </tr>
          <tr>
            <td align="left" valign="bottom">22<hr/></td>
            <td align="left" valign="bottom">GSE18842<hr/></td>
            <td align="left" valign="bottom">20878980<hr/></td>
            <td align="left" valign="bottom">[<xref ref-type="bibr" rid="B35">35</xref>]<hr/></td>
            <td align="left" valign="bottom">Non-Small Cell Lung Cancer<hr/></td>
            <td align="left" valign="bottom">hsa05223<hr/></td>
            <td align="left" valign="bottom">Lung<hr/></td>
          </tr>
          <tr>
            <td align="left" valign="bottom">23<hr/></td>
            <td align="left" valign="bottom">GSE19188<hr/></td>
            <td align="left" valign="bottom">20421987<hr/></td>
            <td align="left" valign="bottom">[<xref ref-type="bibr" rid="B36">36</xref>]<hr/></td>
            <td align="left" valign="bottom">Non-Small Cell Lung Cancer<hr/></td>
            <td align="left" valign="bottom">hsa05223<hr/></td>
            <td align="left" valign="bottom">Lung<hr/></td>
          </tr>
          <tr>
            <td align="left">24</td>
            <td align="left">GSE3585</td>
            <td align="left">17045896</td>
