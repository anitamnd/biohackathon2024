      </p>
      <p>
        <disp-formula id="bmcM3">
          <label>(3)</label>
          <mml:math id="M33" name="1471-2105-15-S7-S4-i30" overflow="scroll">
            <mml:mrow>
              <mml:mstyle class="text">
                <mml:mtext class="textsf">Total</mml:mtext>
              </mml:mstyle>
              <mml:mspace class="tmspace" width="2.77695pt"/>
              <mml:mtext>ÎÎ</mml:mtext>
              <mml:mstyle class="text">
                <mml:mtext class="textsf">G</mml:mtext>
              </mml:mstyle>
              <mml:mo class="MathClass-rel">=</mml:mo>
              <mml:mstyle displaystyle="true">
                <mml:munderover accent="false" accentunder="false">
                  <mml:mrow>
                    <mml:mo> â</mml:mo>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mstyle class="text">
                      <mml:mtext class="textsf">i</mml:mtext>
                    </mml:mstyle>
                    <mml:mo class="MathClass-rel">=</mml:mo>
                    <mml:mn>1</mml:mn>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mstyle class="text">
                      <mml:mtext class="textsf">n</mml:mtext>
                    </mml:mstyle>
                  </mml:mrow>
                </mml:munderover>
              </mml:mstyle>
              <mml:mstyle displaystyle="true">
                <mml:munderover accent="false" accentunder="false">
                  <mml:mrow>
                    <mml:mo> â</mml:mo>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mstyle class="text">
                      <mml:mtext class="textsf">j</mml:mtext>
                    </mml:mstyle>
                    <mml:mo class="MathClass-rel">=</mml:mo>
                    <mml:mn>1</mml:mn>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mstyle class="text">
                      <mml:mtext class="textsf">m</mml:mtext>
                    </mml:mstyle>
                  </mml:mrow>
