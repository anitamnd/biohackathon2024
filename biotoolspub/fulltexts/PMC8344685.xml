}{}\begin{eqnarray*}D \left( r,s \right) = \frac{{N}_{rs}}{L-1} ,r,s\in \{ A,C,D,\ldots ,Y\} \end{eqnarray*}\end{document}</tex-math><mml:math id="mml-NONUM-d2e462"><mml:mstyle displaystyle="true"><mml:mi>D</mml:mi><mml:mfenced separators="" open="(" close=")"><mml:mrow><mml:mi>r</mml:mi><mml:mo>,</mml:mo><mml:mi>s</mml:mi></mml:mrow></mml:mfenced><mml:mo>=</mml:mo><mml:mfrac><mml:mrow><mml:msub><mml:mrow><mml:mi>N</mml:mi></mml:mrow><mml:mrow><mml:mi>r</mml:mi><mml:mi>s</mml:mi></mml:mrow></mml:msub></mml:mrow><mml:mrow><mml:mi>L</mml:mi><mml:mo>−</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:mfrac><mml:mo>,</mml:mo><mml:mi>r</mml:mi><mml:mo>,</mml:mo><mml:mi>s</mml:mi><mml:mo>∈</mml:mo><mml:mrow><mml:mfenced separators="" open="{" close="}"><mml:mi>A</mml:mi><mml:mo>,</mml:mo><mml:mi>C</mml:mi><mml:mo>,</mml:mo><mml:mi>D</mml:mi><mml:mo>,</mml:mo><mml:mo>…</mml:mo><mml:mo>,</mml:mo><mml:mi>Y</mml:mi></mml:mfenced></mml:mrow></mml:mstyle></mml:math></alternatives></disp-formula>
</p>
