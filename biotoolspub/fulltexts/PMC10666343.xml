				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{array}{c}MCC=\frac{{T}_{P}\times {T}_{N}-{F}_{P}\times {F}_{N}}{\sqrt{\left({T}_{P}+{F}_{N}\right)\left({T}_{P}+{F}_{P}\right)\left({T}_{N}+{F}_{P}\right)\left({T}_{N}+{F}_{N}\right)}}\end{array}$$\end{document}</tex-math>
          <mml:math id="M26" display="block">
