    <p id="Par20">
      <disp-formula id="Equ7">
        <label>7</label>
        <alternatives>
          <tex-math id="M25">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
