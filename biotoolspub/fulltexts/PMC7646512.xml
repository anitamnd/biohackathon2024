                        </mml:mrow>
                        <mml:mrow>
                          <mml:msub>
                            <mml:mo mathvariant="italic">max</mml:mo>
                            <mml:mrow>
                              <mml:mi>i</mml:mi>
                              <mml:mo>∈</mml:mo>
                              <mml:mi>V</mml:mi>
                            </mml:mrow>
                          </mml:msub>
                          <mml:mfenced close=")" open="(">
                            <mml:mrow>
                              <mml:msub>
                                <mml:mo mathvariant="italic">log</mml:mo>
                                <mml:mn>2</mml:mn>
                              </mml:msub>
                              <mml:msub>
                                <mml:mi>d</mml:mi>
                                <mml:mi>i</mml:mi>
                              </mml:msub>
                            </mml:mrow>
                          </mml:mfenced>
                          <mml:mo>−</mml:mo>
                          <mml:msub>
                            <mml:mo mathvariant="italic">min</mml:mo>
                            <mml:mrow>
                              <mml:mi>i</mml:mi>
                              <mml:mo>∈</mml:mo>
                              <mml:mi>V</mml:mi>
                            </mml:mrow>
                          </mml:msub>
                          <mml:mfenced close=")" open="(">
                            <mml:mrow>
                              <mml:msub>
                                <mml:mo mathvariant="italic">log</mml:mo>
                                <mml:mn>2</mml:mn>
                              </mml:msub>
                              <mml:msub>
                                <mml:mi>d</mml:mi>
                                <mml:mi>i</mml:mi>
                              </mml:msub>
