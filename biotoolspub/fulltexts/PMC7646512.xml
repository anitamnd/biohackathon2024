                  <mml:mtr>
                    <mml:mtd>
                      <mml:maligngroup/>
                      <mml:mn>1</mml:mn>
                      <mml:mo>,</mml:mo>
                      <mml:msub>
                        <mml:mi>d</mml:mi>
                        <mml:mi>x</mml:mi>
                      </mml:msub>
                      <mml:mo>≤</mml:mo>
                      <mml:mn>2</mml:mn>
                    </mml:mtd>
