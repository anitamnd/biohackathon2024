                        </mml:mrow>
                        <mml:mo>Â·</mml:mo>
                        <mml:mi>r</mml:mi>
                        <mml:mi>c</mml:mi>
                        <mml:mrow>
                          <mml:mo stretchy="false">(</mml:mo>
                          <mml:mrow>
