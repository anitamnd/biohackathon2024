                </mml:mrow>
              </mml:msubsup>
              <mml:mo>|</mml:mo>
              <mml:mo>|</mml:mo>
              <mml:mi mathvariant="normal">F</mml:mi>
              <mml:mrow>
                <mml:mfenced separators="" open="(" close=")">
                  <mml:msub>
                    <mml:mrow>
