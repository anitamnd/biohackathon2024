<sub-article article-type="author-comment" id="pone.0283567.r002">
  <front-stub>
    <article-id pub-id-type="doi">10.1371/journal.pone.0283567.r002</article-id>
    <title-group>
      <article-title>Author response to Decision Letter 0</article-title>
    </title-group>
    <related-article ext-link-type="doi" xlink:href="10.1371/journal.pone.0283567" id="rel-obj002" related-article-type="editor-report"/>
    <custom-meta-group>
