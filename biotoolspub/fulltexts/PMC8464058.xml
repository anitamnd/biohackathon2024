\usepackage{upgreek}
\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$$\begin{equation*}{R_{real}} = {x_{N \times M}}\ + {\alpha _j} \approx M{e_{ij}}\end{equation*}$$\end{document}</tex-math></disp-formula><disp-formula><tex-math id="M15">\documentclass[12pt]{minimal}
\usepackage{amsmath}
