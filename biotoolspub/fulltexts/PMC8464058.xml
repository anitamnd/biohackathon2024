\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$$\begin{equation*}{\rm{R}}_{N \times M}^{\rm{^{\prime}}} = {{\rm{R}}_{N \times M}}\ - \ M{e_{ij}} = {y_{N \times M}}\ + {\varepsilon _{N \times M}}\end{equation*}$$\end{document}</tex-math></disp-formula>Where <inline-formula><tex-math id="M16">\documentclass[12pt]{minimal}
