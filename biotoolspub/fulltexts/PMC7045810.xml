                  <mml:mi>a</mml:mi>
                  <mml:mi>l</mml:mi>
                  <mml:mi>l</mml:mi>
                  <mml:mo> </mml:mo>
                  <mml:mi>p</mml:mi>
                  <mml:mi>o</mml:mi>
                  <mml:mi>s</mml:mi>
                  <mml:mi>s</mml:mi>
                  <mml:mi>i</mml:mi>
                  <mml:mi>b</mml:mi>
                  <mml:mi>l</mml:mi>
                  <mml:mi>e</mml:mi>
                  <mml:mo> </mml:mo>
                  <mml:mi>a</mml:mi>
                  <mml:mi>t</mml:mi>
                  <mml:mi>o</mml:mi>
                  <mml:mi>m</mml:mi>
