                  <mml:mo stretchy="false">)</mml:mo>
                </mml:mrow>
                <mml:mrow>
                  <mml:mi>T</mml:mi>
                  <mml:mi>o</mml:mi>
                  <mml:mi>t</mml:mi>
                  <mml:mi>a</mml:mi>
                  <mml:mi>l</mml:mi>
                  <mml:mo> </mml:mo>
                  <mml:mi>n</mml:mi>
                  <mml:mi>o</mml:mi>
                  <mml:mo>.</mml:mo>
                  <mml:mo> </mml:mo>
