            <tr>
              <td rowspan="1" colspan="1">35 histidines</td>
              <td rowspan="1" colspan="1">14 histidines<break/>
Glu338<break/>
Glu1007<break/>
Asp1031</td>
              <td rowspan="1" colspan="1">Asp1031–Gln756<break/>
His1029–Asp684<break/>
Glu1007–Ser764<break/>
Asp684–His1029<break/>
His1145–His1160<break/>
