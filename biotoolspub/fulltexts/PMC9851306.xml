\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$$ \mathrm{Accuracy}=\frac{TP+ TN}{TP+ TN+ FP+ FN} $$\end{document}</tex-math></disp-formula><disp-formula id="deqn07"><tex-math notation="LaTeX" id="DmEquation7">\documentclass[12pt]{minimal}
