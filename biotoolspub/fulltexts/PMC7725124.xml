                        <mml:msup>
                          <mml:mrow>
                            <mml:mi>m</mml:mi>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mo>+</mml:mo>
                          </mml:mrow>
                        </mml:msup>
                      </mml:mrow>
                    </mml:mfenced>
                    <mml:mo>−</mml:mo>
                    <mml:munder>
                      <mml:mrow>
                        <mml:mo mathsize="big">∑</mml:mo>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mi>i</mml:mi>
                        <mml:mo>≤</mml:mo>
                        <mml:mi>k</mml:mi>
                      </mml:mrow>
                    </mml:munder>
                    <mml:mi>E</mml:mi>
                    <mml:mfenced close=")" open="(">
                      <mml:mrow>
                        <mml:mi>c</mml:mi>
                        <mml:mo>,</mml:mo>
                        <mml:msubsup>
                          <mml:mrow>
                            <mml:mi>m</mml:mi>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mi>i</mml:mi>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mo>−</mml:mo>
                          </mml:mrow>
                        </mml:msubsup>
                      </mml:mrow>
                    </mml:mfenced>
                  </mml:mtd>
                  <mml:mtd class="align-2">
                    <mml:mspace width="1em"/>
                    <mml:mo>∀</mml:mo>
                    <mml:mi>c</mml:mi>
                    <mml:mo>∈</mml:mo>
                    <mml:mi>C</mml:mi>
                    <mml:mo>,</mml:mo>
                    <mml:mspace width="1em"/>
                    <mml:mi>m</mml:mi>
                    <mml:mo>∈</mml:mo>
                    <mml:mi>M</mml:mi>
                    <mml:mspace width="2em"/>
                  </mml:mtd>
                </mml:mtr>
              </mml:mtable>
            </mml:math>
            <graphic xlink:href="12859_2020_3736_Article_Equ4.gif" position="anchor"/>
          </alternatives>
        </disp-formula>
      </p>
      <p>
        <disp-formula id="Equ5">
          <label>5</label>
          <alternatives>
            <tex-math id="M35">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document} $$\begin{array}{*{20}l} w(c,m) = \left(1 - \alpha\right) F(c,m) + \beta \left(1 - F(c,m) \right)\quad &amp; \text{ if} I(c,m) = 1 \end{array} $$ \end{document}</tex-math>
