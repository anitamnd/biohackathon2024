                                        <mml:msup>
                                          <mml:mrow>
                                            <mml:mo>(</mml:mo>
                                            <mml:mstyle mathvariant="bold" mathsize="normal">
                                              <mml:mi>Q</mml:mi>
                                            </mml:mstyle>
                                            <mml:mo>′</mml:mo>
                                            <mml:mstyle mathvariant="bold" mathsize="normal">
                                              <mml:mi>Q</mml:mi>
                                            </mml:mstyle>
                                            <mml:mo>)</mml:mo>
