                      </mml:mtd>
                      <mml:mtd columnalign="center">
                        <mml:mrow>
                          <mml:mn mathvariant="bold">1</mml:mn>
                          <mml:mo mathvariant="bold">′</mml:mo>
                        </mml:mrow>
                      </mml:mtd>
                    </mml:mtr>
                    <mml:mtr columnalign="center">
                      <mml:mtd columnalign="center">
                        <mml:mn mathvariant="bold">1</mml:mn>
                      </mml:mtd>
                      <mml:mtd columnalign="center">
                        <mml:mrow>
                          <mml:mi mathvariant="bold">I</mml:mi>
                          <mml:mo mathvariant="bold">+</mml:mo>
                          <mml:msup>
                            <mml:mi mathvariant="bold">λ</mml:mi>
                            <mml:mfenced open="(" close=")">
                              <mml:mrow>
                                <mml:mn>2</mml:mn>
                                <mml:mi>a</mml:mi>
                              </mml:mrow>
                            </mml:mfenced>
                          </mml:msup>
                          <mml:msup>
                            <mml:mi mathvariant="bold">A</mml:mi>
                            <mml:mrow>
                              <mml:mo mathvariant="bold">-</mml:mo>
                              <mml:mn mathvariant="bold">1</mml:mn>
                            </mml:mrow>
                          </mml:msup>
                        </mml:mrow>
                      </mml:mtd>
                    </mml:mtr>
                  </mml:mtable>
                </mml:mfenced>
                <mml:mspace width=".5em"/>
                <mml:mfenced open="[" close="]">
