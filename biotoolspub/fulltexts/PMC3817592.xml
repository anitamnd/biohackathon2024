                  </mml:mtable>
                </mml:mfenced>
                <mml:mo>=</mml:mo>
                <mml:mfenced open="[" close="]">
                  <mml:mtable columnalign="center">
                    <mml:mtr columnalign="center">
                      <mml:mtd columnalign="center">
                        <mml:mrow>
                          <mml:mn mathvariant="bold">1</mml:mn>
                          <mml:mo mathvariant="bold">′</mml:mo>
                          <mml:mi mathvariant="bold">y</mml:mi>
