                        <mml:mn>2</mml:mn>
                        <mml:mi>a</mml:mi>
                      </mml:mrow>
                    </mml:mfenced>
                  </mml:msup>
                </mml:mfenced>
                <mml:mo>=</mml:mo>
                <mml:msubsup>
