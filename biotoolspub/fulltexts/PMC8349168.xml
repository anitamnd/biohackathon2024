              </mml:mrow>
              <mml:mo>=</mml:mo>
              <mml:mrow>
                <mml:mo stretchy="false">[</mml:mo>
                <mml:mrow>
                  <mml:msub>
                    <mml:mi>F</mml:mi>
                    <mml:mrow>
                      <mml:mn>1</mml:mn>
                      <mml:mspace width="thickmathspace"/>
                    </mml:mrow>
