            <col valign="top" align="left" span="1"/>
          </colgroup>
          <thead>
            <tr>
              <th rowspan="1" colspan="1">Models</th>
              <th align="center" rowspan="1" colspan="1">VGG13</th>
              <th align="center" rowspan="1" colspan="1">Alexnet</th>
              <th align="center" rowspan="1" colspan="1">Resnet18</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td rowspan="1" colspan="1">Epoch</td>
              <td rowspan="1" colspan="1">500</td>
              <td rowspan="1" colspan="1">500</td>
              <td rowspan="1" colspan="1">500</td>
            </tr>
            <tr>
              <td rowspan="1" colspan="1">Loss</td>
