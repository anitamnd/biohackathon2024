\usepackage{wasysym}
\usepackage{amsfonts}
\usepackage{amssymb}
\usepackage{amsbsy}
\usepackage{upgreek}
\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$$\begin{equation*}\sigma {\rm{\ }} = \frac{{min\left( {svd\left( X \right)} \right)}}{{SNR\sqrt {{\chi ^{ - 2}}\left( {\alpha ,{\rm{\ }}NM} \right)} }}\ \end{equation*}$$\end{document}</tex-math></disp-formula>In Eq. <xref rid="M1" ref-type="disp-formula">1</xref>, <inline-formula><tex-math id="M0001a" notation="LaTeX">\documentclass[12pt]{minimal}
\usepackage{amsmath}
