            <colgroup span="1">
              <col align="left" valign="middle" span="1"/>
              <col align="left" valign="middle" span="1"/>
            </colgroup>
            <tbody>
              <tr>
                <td align="left" rowspan="1" colspan="1">Traits with more S-MultiXcan-significant associations</td>
                <td align="right" rowspan="1" colspan="1">102</td>
              </tr>
              <tr>
                <td align="left" rowspan="1" colspan="1">Traits with more S-PrediXcan-significant associations</td>
                <td align="right" rowspan="1" colspan="1">22</td>
              </tr>
              <tr>
                <td align="left" rowspan="1" colspan="1">Tied traits</td>
                <td align="right" rowspan="1" colspan="1">14</td>
              </tr>
