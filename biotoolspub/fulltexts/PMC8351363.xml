                    <mml:mo>exp</mml:mo>
                    <mml:mfenced close=")" open="(">
                      <mml:mrow>
                        <mml:mo>−</mml:mo>
                        <mml:msub>
                          <mml:mrow>
                            <mml:mi>γ</mml:mi>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mn>2</mml:mn>
