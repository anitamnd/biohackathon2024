                        </mml:msubsup>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:msup>
                          <mml:mrow>
                            <mml:mo stretchy="false">(</mml:mo>
                            <mml:msubsup>
                              <mml:mo>Î </mml:mo>
                              <mml:mrow>
                                <mml:mi>j</mml:mi>
                                <mml:mo>=</mml:mo>
                                <mml:mn>1</mml:mn>
                              </mml:mrow>
                              <mml:mi>p</mml:mi>
