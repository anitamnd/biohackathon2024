              <td rowspan="1" colspan="1">UniProt</td>
            </tr>
            <tr>
              <td rowspan="1" colspan="1">Cancer variants</td>
              <td rowspan="1" colspan="1">Cancer missense variants in human</td>
              <td rowspan="1" colspan="1">COSMIC</td>
            </tr>
