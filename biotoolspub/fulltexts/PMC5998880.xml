                      </mml:mrow>
                    </mml:mfenced>
                    <mml:mo>+</mml:mo>
                    <mml:msub>
                      <mml:mi>λ</mml:mi>
                      <mml:mi>G</mml:mi>
                    </mml:msub>
                    <mml:munderover>
