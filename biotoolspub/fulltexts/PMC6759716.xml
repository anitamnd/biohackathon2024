                              <mml:mrow>
                                <mml:mstyle mathvariant="bold">
                                  <mml:mn>1</mml:mn>
                                </mml:mstyle>
                                <mml:mo>,</mml:mo>
                                <mml:mstyle mathvariant="bold">
                                  <mml:mn>2</mml:mn>
                                </mml:mstyle>
                              </mml:mrow>
                            </mml:msub>
                            <mml:mo>×</mml:mo>
                            <mml:mstyle mathvariant="bold-italic">
                              <mml:mi>b</mml:mi>
