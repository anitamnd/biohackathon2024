                              <mml:mstyle mathvariant="bold-italic">
                                <mml:mi>f</mml:mi>
                              </mml:mstyle>
                              <mml:mstyle mathvariant="bold">
                                <mml:mn>2</mml:mn>
                              </mml:mstyle>
                            </mml:msub>
                          </mml:mrow>
                        </mml:msup>
                      </mml:mrow>
                    </mml:mtd>
                    <mml:mtd>
                      <mml:mo>···</mml:mo>
                    </mml:mtd>
                    <mml:mtd>
                      <mml:mrow>
                        <mml:msup>
                          <mml:mstyle mathvariant="bold">
                            <mml:mn>2</mml:mn>
                          </mml:mstyle>
                          <mml:mrow>
