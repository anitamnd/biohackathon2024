            </mml:mrow>
          </mml:msub>
          <mml:mo>=</mml:mo>
          <mml:msub>
            <mml:mrow>
              <mml:mo>|</mml:mo>
              <mml:mrow>
                <mml:mtable>
                  <mml:mtr>
                    <mml:mtd>
                      <mml:mrow>
                        <mml:msup>
                          <mml:mstyle mathvariant="bold">
                            <mml:mn>2</mml:mn>
                          </mml:mstyle>
                          <mml:mrow>
                            <mml:msub>
                              <mml:mstyle mathvariant="bold-italic">
                                <mml:mi>p</mml:mi>
                              </mml:mstyle>
                              <mml:mrow>
                                <mml:mstyle mathvariant="bold">
                                  <mml:mn>1</mml:mn>
                                </mml:mstyle>
                                <mml:mo>,</mml:mo>
                                <mml:mstyle mathvariant="bold">
                                  <mml:mn>1</mml:mn>
