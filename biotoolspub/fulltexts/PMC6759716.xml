                <mml:mo>…</mml:mo>
                <mml:msub>
                  <mml:mrow>
                    <mml:mstyle mathvariant="bold-italic">
                      <mml:mi>T</mml:mi>
                    </mml:mstyle>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mstyle mathvariant="bold-italic">
                      <mml:mi>i</mml:mi>
                    </mml:mstyle>
                  </mml:mrow>
