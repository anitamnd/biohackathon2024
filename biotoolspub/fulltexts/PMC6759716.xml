                <mml:msub>
                  <mml:mrow>
                    <mml:mstyle mathvariant="bold-italic">
                      <mml:mi>T</mml:mi>
                    </mml:mstyle>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mstyle mathvariant="bold">
                      <mml:mn>2</mml:mn>
                    </mml:mstyle>
                  </mml:mrow>
                </mml:msub>
                <mml:msub>
                  <mml:mrow>
                    <mml:mstyle mathvariant="bold-italic">
                      <mml:mi>T</mml:mi>
