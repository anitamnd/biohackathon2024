                            </mml:msub>
                            <mml:mo>×</mml:mo>
                            <mml:mstyle mathvariant="bold-italic">
                              <mml:mi>b</mml:mi>
                            </mml:mstyle>
                            <mml:msub>
                              <mml:mstyle mathvariant="bold-italic">
                                <mml:mi>f</mml:mi>
                              </mml:mstyle>
                              <mml:mstyle mathvariant="bold">
                                <mml:mn>1</mml:mn>
                              </mml:mstyle>
                            </mml:msub>
                          </mml:mrow>
                        </mml:msup>
                      </mml:mrow>
                    </mml:mtd>
                    <mml:mtd>
                      <mml:mrow>
                        <mml:msup>
                          <mml:mstyle mathvariant="bold">
