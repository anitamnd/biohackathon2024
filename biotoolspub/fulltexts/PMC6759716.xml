                    </mml:mstyle>
                  </mml:mrow>
                </mml:msub>
              </mml:mtd>
              <mml:mtd>
                <mml:mo>=</mml:mo>
              </mml:mtd>
              <mml:mtd>
                <mml:msub>
                  <mml:mrow>
                    <mml:mstyle mathvariant="bold-italic">
