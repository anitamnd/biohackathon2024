                              <mml:mi>i</mml:mi>
                            </mml:mstyle>
                          </mml:msub>
                          <mml:mrow>
                            <mml:mo>(</mml:mo>
                            <mml:mstyle mathvariant="bold-italic">
                              <mml:mi>k</mml:mi>
                            </mml:mstyle>
                            <mml:mo>)</mml:mo>
                          </mml:mrow>
                          <mml:mo>−</mml:mo>
                          <mml:msub>
                            <mml:mrow>
                              <mml:mover accent="true">
                                <mml:mstyle mathvariant="bold-italic">
                                  <mml:mi>f</mml:mi>
                                </mml:mstyle>
                                <mml:mo stretchy="true">¯</mml:mo>
                              </mml:mover>
