                    </mml:msub>
                  </mml:mrow>
                  <mml:mo>-</mml:mo>
                  <mml:mrow>
                    <mml:mover accent="true">
                      <mml:mi>S</mml:mi>
                      <mml:mo stretchy="false">Â¯</mml:mo>
                    </mml:mover>
                    <mml:mo>â¢</mml:mo>
                    <mml:msub>
                      <mml:mrow>
