                        <mml:mi>r</mml:mi>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mi>e</mml:mi>
                        <mml:mo>â¢</mml:mo>
                        <mml:mi>x</mml:mi>
                        <mml:mo>â¢</mml:mo>
