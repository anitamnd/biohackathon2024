            <tr>
              <td valign="top" align="left" rowspan="1" colspan="1">lncRNA<xref ref-type="table-fn" rid="TN2"><sup>b</sup></xref></td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.898</td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.892</td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.892</td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.893</td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.893</td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.894</td>
              <td valign="top" align="center" rowspan="1" colspan="1">0.894</td>
