            <mml:mi>min</mml:mi>
            <mml:mo>⁡</mml:mo>
            <mml:mo stretchy="false">(</mml:mo>
            <mml:mover accent="true">
              <mml:mi>r</mml:mi>
              <mml:mo>˜</mml:mo>
            </mml:mover>
            <mml:mo>−</mml:mo>
            <mml:mn>1</mml:mn>
            <mml:mo>,</mml:mo>
            <mml:mover accent="true">
