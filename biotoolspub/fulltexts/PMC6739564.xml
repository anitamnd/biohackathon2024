      <disp-formula>
        <label>(11),</label>
        <mml:math id="M7">
          <mml:mrow>
            <mml:mover accent="true">
              <mml:mi>r</mml:mi>
              <mml:mo>˜</mml:mo>
            </mml:mover>
            <mml:mo>=</mml:mo>
            <mml:mi>r</mml:mi>
            <mml:mo>−</mml:mo>
            <mml:mrow>
              <mml:mo>(</mml:mo>
              <mml:mrow>
                <mml:mfrac>
                  <mml:mrow>
                    <mml:msup>
                      <mml:mrow>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>r</mml:mi>
                        <mml:mo>−</mml:mo>
                        <mml:mn>1</mml:mn>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
