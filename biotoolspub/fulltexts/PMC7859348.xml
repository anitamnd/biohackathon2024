      <disp-formula id="S2.Ex2">
        <mml:math id="M2">
          <mml:mrow>
            <mml:mi>k</mml:mi>
            <mml:mo>=</mml:mo>
            <mml:mrow>
              <mml:mn>1</mml:mn>
              <mml:mrow>
                <mml:mo>(</mml:mo>
                <mml:mrow>
                  <mml:mstyle displaystyle="true">
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mtext>N</mml:mtext>
                        <mml:mrow>
                          <mml:mo>[</mml:mo>
                          <mml:mtext>AxA</mml:mtext>
                          <mml:mo>]</mml:mo>
