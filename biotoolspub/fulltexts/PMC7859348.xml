                          <mml:mtext>AA</mml:mtext>
                          <mml:mo>]</mml:mo>
                        </mml:mrow>
                      </mml:mrow>
                      <mml:msub>
                        <mml:mi>N</mml:mi>
                        <mml:mn>0</mml:mn>
                      </mml:msub>
                    </mml:mfrac>
                  </mml:mstyle>
                </mml:mrow>
                <mml:mo>,</mml:mo>
                <mml:mrow>
                  <mml:mstyle displaystyle="true">
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mtext>N</mml:mtext>
                        <mml:mrow>
                          <mml:mo>[</mml:mo>
                          <mml:mtext>AC</mml:mtext>
                          <mml:mo>]</mml:mo>
                        </mml:mrow>
                      </mml:mrow>
                      <mml:msub>
                        <mml:mi>N</mml:mi>
                        <mml:mn>0</mml:mn>
                      </mml:msub>
                    </mml:mfrac>
                  </mml:mstyle>
                </mml:mrow>
                <mml:mo>,</mml:mo>
