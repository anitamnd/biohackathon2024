                <mml:mi>A</mml:mi>
                <mml:msub>
                  <mml:mrow>
                    <mml:mi>V</mml:mi>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mi>w</mml:mi>
                  </mml:mrow>
                </mml:msub>
                <mml:mrow>
                  <mml:mo stretchy="false">(</mml:mo>
