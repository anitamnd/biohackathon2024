                </mml:mrow>
                <mml:mo>=</mml:mo>
                <mml:msqrt>
                  <mml:mrow>
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mi>T</mml:mi>
                      </mml:mrow>
                    </mml:mfrac>
                    <mml:msubsup>
                      <mml:mrow>
                        <mml:mo>∑</mml:mo>
