                        <mml:mo>≠</mml:mo>
                        <mml:mi>x</mml:mi>
                      </mml:mrow>
                      <mml:mi>N</mml:mi>
                    </mml:msubsup>
                    <mml:mrow>
                      <mml:mrow>
                        <mml:msub>
                          <mml:mi>C</mml:mi>
                          <mml:mrow>
