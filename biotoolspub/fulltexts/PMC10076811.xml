            </mml:msub>
            <mml:mfenced>
              <mml:mrow>
                <mml:mn>1</mml:mn>
                <mml:mo>−</mml:mo>
                <mml:mi>γ</mml:mi>
              </mml:mrow>
            </mml:mfenced>
            <mml:mo>+</mml:mo>
            <mml:mfenced>
              <mml:mrow>
                <mml:mi>S</mml:mi>
