                  </mml:mrow>
                </mml:mfenced>
                <mml:mfenced>
                  <mml:mrow>
                    <mml:mn>1</mml:mn>
                    <mml:mo>−</mml:mo>
                    <mml:mi>α</mml:mi>
                  </mml:mrow>
                </mml:mfenced>
                <mml:mo>+</mml:mo>
                <mml:mi>S</mml:mi>
                <mml:mi>I</mml:mi>
                <mml:msub>
                  <mml:mi>M</mml:mi>
