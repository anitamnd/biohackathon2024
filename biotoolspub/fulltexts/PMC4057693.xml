                                            <mml:mo>+</mml:mo>
                                          </mml:msubsup>
                                        </mml:mrow>
                                      </mml:mtd>
                                      <mml:mtd>
                                        <mml:mrow>
                                          <mml:msubsup>
                                            <mml:mrow>
                                              <mml:mover accent="true">
                                                <mml:mi mathvariant="bold">ψ</mml:mi>
                                                <mml:mo>¯</mml:mo>
