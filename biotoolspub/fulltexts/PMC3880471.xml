				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$ \begin{array}{l}\mathcal{U}\left({x}_i\right)=\mu \left[{x}_i\right]+10\sqrt{\left(1+{\sigma}^2\left[{x}_i\right]\right)}\hfill \\ {}\mathcal{B}\left({x}_i\right)=\begin{array}{ll}\mu \left[{x}_i\right]-10\sqrt{\left(1+{\sigma}^2\left[{x}_i\right]\right)},\hfill &amp; \mu \left[{x}_i\right]-10\sqrt{\left(1+{\sigma}^2\left[{x}_i\right]\right)}&gt;0\hfill \\ {}0,\hfill &amp; \mu \left[{x}_i\right]-10\sqrt{\left(1+{\sigma}^2\left[{x}_i\right]\right)}\le 0\hfill \end{array}\hfill \end{array} $$\end{document}</tex-math><mml:math id="M32"><mml:mtable columnalign="left"><mml:mtr columnalign="left"><mml:mtd columnalign="left"><mml:mi mathvariant="script">U</mml:mi><mml:mfenced close=")" open="("><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced><mml:mo>=</mml:mo><mml:mi>μ</mml:mi><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced><mml:mo>+</mml:mo><mml:mn>10</mml:mn><mml:msqrt><mml:mfenced close=")" open="("><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:msup><mml:mi>σ</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced></mml:mrow></mml:mfenced></mml:msqrt></mml:mtd></mml:mtr><mml:mtr columnalign="left"><mml:mtd columnalign="left"><mml:mi mathvariant="script">B</mml:mi><mml:mfenced close=")" open="("><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced><mml:mo>=</mml:mo><mml:mfenced close="" open=""><mml:mtable columnalign="left"><mml:mtr columnalign="left"><mml:mtd columnalign="left"><mml:mi>μ</mml:mi><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced><mml:mo>−</mml:mo><mml:mn>10</mml:mn><mml:msqrt><mml:mfenced close=")" open="("><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:msup><mml:mi>σ</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced></mml:mrow></mml:mfenced></mml:msqrt><mml:mo>,</mml:mo></mml:mtd><mml:mtd columnalign="left"><mml:mi>μ</mml:mi><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced><mml:mo>−</mml:mo><mml:mn>10</mml:mn><mml:msqrt><mml:mfenced close=")" open="("><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:msup><mml:mi>σ</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced></mml:mrow></mml:mfenced></mml:msqrt><mml:mo>&gt;</mml:mo><mml:mn>0</mml:mn></mml:mtd></mml:mtr><mml:mtr columnalign="left"><mml:mtd columnalign="left"><mml:mn>0</mml:mn><mml:mo>,</mml:mo></mml:mtd><mml:mtd columnalign="left"><mml:mi>μ</mml:mi><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced><mml:mo>−</mml:mo><mml:mn>10</mml:mn><mml:msqrt><mml:mfenced close=")" open="("><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:msup><mml:mi>σ</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mfenced close="]" open="["><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mfenced></mml:mrow></mml:mfenced></mml:msqrt><mml:mo>≤</mml:mo><mml:mn>0</mml:mn></mml:mtd></mml:mtr></mml:mtable></mml:mfenced></mml:mtd></mml:mtr></mml:mtable></mml:math><graphic xlink:href="13361_2013_733_Article_Equ6.gif" position="anchor"/></alternatives></disp-formula>
