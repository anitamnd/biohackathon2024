              <mml:mi>v</mml:mi>
            </mml:mrow>
          </mml:msub>
          <mml:mo>=</mml:mo>
          <mml:mrow>
            <mml:mo>{</mml:mo>
            <mml:mtable columnalign="left">
              <mml:mtr columnalign="left">
                <mml:mtd columnalign="left">
                  <mml:mfrac>
                    <mml:mrow>
                      <mml:mrow>
                        <mml:mo>(</mml:mo>
                        <mml:mtable>
                          <mml:mtr>
                            <mml:mtd>
                              <mml:mi>l</mml:mi>
                            </mml:mtd>
                          </mml:mtr>
                          <mml:mtr>
                            <mml:mtd>
                              <mml:mi>v</mml:mi>
                            </mml:mtd>
                          </mml:mtr>
                        </mml:mtable>
                        <mml:mo>)</mml:mo>
                      </mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:mtable>
                        <mml:mtr>
                          <mml:mtd>
                            <mml:mrow>
                              <mml:mn>2</mml:mn>
                              <mml:mi>m</mml:mi>
                              <mml:mo>-</mml:mo>
                              <mml:mi>l</mml:mi>
                            </mml:mrow>
                          </mml:mtd>
                        </mml:mtr>
                        <mml:mtr>
                          <mml:mtd>
                            <mml:mrow>
                              <mml:mn>2</mml:mn>
                              <mml:mo>-</mml:mo>
                              <mml:mi>v</mml:mi>
                            </mml:mrow>
                          </mml:mtd>
                        </mml:mtr>
                      </mml:mtable>
                      <mml:mo>)</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mo>(</mml:mo>
                      <mml:mtable>
                        <mml:mtr>
                          <mml:mtd>
                            <mml:mrow>
                              <mml:mn>2</mml:mn>
                              <mml:mi>m</mml:mi>
                            </mml:mrow>
                          </mml:mtd>
                        </mml:mtr>
                        <mml:mtr>
                          <mml:mtd>
                            <mml:mn>2</mml:mn>
