            <tr>
              <td rowspan="1" colspan="1">lncRNA interactions</td>
              <td rowspan="1" colspan="1">658 171</td>
            </tr>
            <tr>
              <td rowspan="1" colspan="1">miRNA interactions</td>
              <td rowspan="1" colspan="1">488 025</td>
            </tr>
            <tr>
