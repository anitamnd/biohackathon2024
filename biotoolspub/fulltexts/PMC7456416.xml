				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \Delta [\theta ]_k^{\mathrm{{spect}}} = \sqrt{\frac{\sum _l{([\theta ]_{kl} - [\theta ]_{l}^{\mathrm{{correct}}})}^2}{\sum _l{([\theta ]_{l}^{\mathrm{{correct}}})}^2}}, \end{aligned}$$\end{document}</tex-math><mml:math id="M116" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi mathvariant="normal">Δ</mml:mi><mml:msubsup><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:mi>θ</mml:mi><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mi>k</mml:mi><mml:mi mathvariant="normal">spect</mml:mi></mml:msubsup><mml:mo>=</mml:mo><mml:msqrt><mml:mfrac><mml:mrow><mml:munder><mml:mo>∑</mml:mo><mml:mi>l</mml:mi></mml:munder><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:mi>θ</mml:mi><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mrow><mml:mi mathvariant="italic">kl</mml:mi></mml:mrow></mml:msub><mml:mo>-</mml:mo><mml:msubsup><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:mi>θ</mml:mi><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mrow><mml:mi>l</mml:mi></mml:mrow><mml:mi mathvariant="normal">correct</mml:mi></mml:msubsup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup></mml:mrow><mml:mrow><mml:munder><mml:mo>∑</mml:mo><mml:mi>l</mml:mi></mml:munder><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msubsup><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:mi>θ</mml:mi><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mrow><mml:mi>l</mml:mi></mml:mrow><mml:mi mathvariant="normal">correct</mml:mi></mml:msubsup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup></mml:mrow></mml:mfrac></mml:msqrt><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="249_2020_1457_Article_Equ5.gif" position="anchor"/></alternatives></disp-formula>where <inline-formula id="IEq29"><alternatives><tex-math id="M117">\documentclass[12pt]{minimal}
