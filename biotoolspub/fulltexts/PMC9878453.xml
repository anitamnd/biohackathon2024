                <mml:mi mathvariant="normal">FP</mml:mi>
              </mml:mrow>
            </mml:mfrac>
            <mml:mo>×</mml:mo>
            <mml:mn>100</mml:mn>
          </mml:mrow>
        </mml:math>
      </disp-formula>
      <disp-formula id="EQ2">
        <label>(2)</label>
        <mml:math id="M2" overflow="scroll">
          <mml:mrow>
            <mml:mspace width="thickmathspace"/>
            <mml:mi mathvariant="normal">Recall</mml:mi>
            <mml:mo>=</mml:mo>
