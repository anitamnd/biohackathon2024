\usepackage{amssymb} 
\usepackage{amsbsy}
\usepackage{upgreek}
\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}\begin{eqnarray*}\mathrm{var} \left( \hat {\beta } \right) ={\hat {A}}^{-1}\hat {B}({\hat {A}}^{-1})^{T} \end{eqnarray*}\end{document}</tex-math><mml:math id="mml-NONUM-d2e1530"><mml:mstyle displaystyle="true"><mml:mi mathvariant="normal">var</mml:mi><mml:mfenced separators="" open="(" close=")"><mml:mrow><mml:mover accent="true"><mml:mrow><mml:mi>β</mml:mi></mml:mrow><mml:mrow><mml:mo> ˆ</mml:mo></mml:mrow></mml:mover></mml:mrow></mml:mfenced><mml:mo>=</mml:mo><mml:msup><mml:mrow><mml:mover accent="true"><mml:mrow><mml:mi>A</mml:mi></mml:mrow><mml:mrow><mml:mo> ˆ</mml:mo></mml:mrow></mml:mover></mml:mrow><mml:mrow><mml:mo>−</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msup><mml:mover accent="true"><mml:mrow><mml:mi>B</mml:mi></mml:mrow><mml:mrow><mml:mo> ˆ</mml:mo></mml:mrow></mml:mover><mml:msup><mml:mrow><mml:mrow><mml:mfenced separators="" open="(" close=")"><mml:msup><mml:mrow><mml:mover accent="true"><mml:mrow><mml:mi>A</mml:mi></mml:mrow><mml:mrow><mml:mo> ˆ</mml:mo></mml:mrow></mml:mover></mml:mrow><mml:mrow><mml:mo>−</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msup></mml:mfenced></mml:mrow></mml:mrow><mml:mrow><mml:mi>T</mml:mi></mml:mrow></mml:msup></mml:mstyle></mml:math></alternatives></disp-formula>
</p>
      <p>where <inline-formula><alternatives><inline-graphic xlink:href="peerj-08-10557-i004.jpg"/><tex-math id="M16">\documentclass[12pt]{minimal}
