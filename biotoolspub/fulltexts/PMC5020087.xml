                  <mml:mi>y</mml:mi>
                  <mml:mo>=</mml:mo>
                  <mml:mn>100000</mml:mn>
                  <mml:mo>*</mml:mo>
                  <mml:mi>Σ</mml:mi>
                  <mml:mtext> </mml:mtext>
                  <mml:mo stretchy="false">(</mml:mo>
                  <mml:mi>S</mml:mi>
                  <mml:mi>S</mml:mi>
                  <mml:mi>R</mml:mi>
                  <mml:mo stretchy="false">)</mml:mo>
                  <mml:mo>/</mml:mo>
                  <mml:mo stretchy="false">(</mml:mo>
                  <mml:mi>L</mml:mi>
                  <mml:mi>e</mml:mi>
                  <mml:mi>n</mml:mi>
                  <mml:mi>g</mml:mi>
