                    <mml:mo stretchy="false">(</mml:mo>
                    <mml:mrow>
                      <mml:mn>1</mml:mn>
                      <mml:mo>-</mml:mo>
                      <mml:mi mathvariant="normal">α</mml:mi>
                    </mml:mrow>
                    <mml:mo stretchy="false">)</mml:mo>
                  </mml:mrow>
                  <mml:mo>⁢</mml:mo>
                  <mml:mover accent="true">
                    <mml:mi>r</mml:mi>
                    <mml:mo stretchy="false">¯</mml:mo>
                  </mml:mover>
