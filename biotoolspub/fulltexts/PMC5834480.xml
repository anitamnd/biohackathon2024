                    <mml:mi>e</mml:mi>
                    <mml:mo> </mml:mo>
                    <mml:mi>f</mml:mi>
                    <mml:mi>r</mml:mi>
                    <mml:mi>a</mml:mi>
                    <mml:mi>c</mml:mi>
                    <mml:mi>t</mml:mi>
                    <mml:mi>i</mml:mi>
                    <mml:mi>o</mml:mi>
                    <mml:mi>n</mml:mi>
                    <mml:mo stretchy="false">(</mml:mo>
                    <mml:mi>i</mml:mi>
                    <mml:mo stretchy="false">)</mml:mo>
                    <mml:mo>=</mml:mo>
