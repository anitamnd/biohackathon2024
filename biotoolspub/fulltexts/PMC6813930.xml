                  <mml:mn>0</mml:mn>
                  <mml:mo>.</mml:mo>
                  <mml:mn>009</mml:mn>
                  <mml:msubsup>
                    <mml:mrow>
                      <mml:mi>C</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
