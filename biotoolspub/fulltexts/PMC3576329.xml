                        <mml:mn>1</mml:mn>
                      </mml:mtd>
                    </mml:mtr>
                    <mml:mtr>
                      <mml:mtd>
                        <mml:munder>
                          <mml:mrow>
                            <mml:mtext>arg max</mml:mtext>
                          </mml:mrow>
                          <mml:mrow>
