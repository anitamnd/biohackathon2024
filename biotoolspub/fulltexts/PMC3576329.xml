                            <mml:mi>S</mml:mi>
                          </mml:mrow>
                        </mml:munder>
                        <mml:mstyle mathvariant="sans-serif">
                          <mml:mi>P</mml:mi>
                        </mml:mstyle>
                        <mml:mo>(</mml:mo>
                        <mml:msub>
