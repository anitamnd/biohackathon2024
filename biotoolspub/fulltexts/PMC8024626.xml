                    </mml:mrow>
                  </mml:msub>
                  <mml:mo>~</mml:mo>
                  <mml:mtext class="textrm" mathvariant="normal">N</mml:mtext>
                  <mml:mrow>
                    <mml:mo stretchy="false">(</mml:mo>
                    <mml:mrow>
                      <mml:msub>
                        <mml:mrow>
                          <mml:mi>Î¼</mml:mi>
