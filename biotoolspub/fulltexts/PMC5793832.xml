\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}${\rm{C - score\ }} = \ln ( {Z*\frac{M}{{RMSD}}} )$\end{document}</tex-math></inline-formula>, <inline-formula><tex-math id="M4">\documentclass[12pt]{minimal}
\usepackage{amsmath}
\usepackage{wasysym} 
\usepackage{amsfonts} 
