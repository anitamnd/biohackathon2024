                  <mml:mtd class="eqnarray-3">
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mn>2</mml:mn>
                        <mml:mtext mathvariant="italic">TP</mml:mtext>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mn>2</mml:mn>
                        <mml:mtext mathvariant="italic">TP</mml:mtext>
                        <mml:mo>+</mml:mo>
                        <mml:mtext mathvariant="italic">FP</mml:mtext>
                        <mml:mo>+</mml:mo>
