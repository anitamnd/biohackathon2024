                                <mml:mo>,</mml:mo>
                                <mml:msub>
                                  <mml:mi>h</mml:mi>
                                  <mml:mi>j</mml:mi>
                                </mml:msub>
                              </mml:mfenced>
                            </mml:mrow>
                          </mml:msup>
                          <mml:mrow>
                            <mml:munder>
                              <mml:mo movablelimits="false">â</mml:mo>
                              <mml:mrow>
                                <mml:mi>k</mml:mi>
                                <mml:mo>â</mml:mo>
                                <mml:mi>N</mml:mi>
                                <mml:mo stretchy="false">(</mml:mo>
                                <mml:mi>i</mml:mi>
                                <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                            </mml:munder>
                            <mml:mi>a</mml:mi>
                            <mml:mrow>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:msub>
                                <mml:mi>h</mml:mi>
                                <mml:mi>k</mml:mi>
                              </mml:msub>
                              <mml:msub>
                                <mml:mi>h</mml:mi>
                                <mml:mi>i</mml:mi>
