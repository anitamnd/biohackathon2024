              <mml:mo class="MathClass-rel">=</mml:mo>
              <mml:mfrac>
                <mml:mrow>
                  <mml:mi>T</mml:mi>
                  <mml:mi>P</mml:mi>
                  <mml:mspace class="thinspace" width="0.3em"/>
                  <mml:mo class="MathClass-bin">⋅</mml:mo>
                  <mml:mspace class="thinspace" width="0.3em"/>
                  <mml:mi>T</mml:mi>
                  <mml:mi>N</mml:mi>
                  <mml:mspace class="thinspace" width="0.3em"/>
                  <mml:mo class="MathClass-bin">-</mml:mo>
                  <mml:mspace class="thinspace" width="0.3em"/>
                  <mml:mi>F</mml:mi>
                  <mml:mi>P</mml:mi>
                  <mml:mspace class="thinspace" width="0.3em"/>
                  <mml:mo class="MathClass-bin">⋅</mml:mo>
                  <mml:mspace class="thinspace" width="0.3em"/>
                  <mml:mi>F</mml:mi>
                  <mml:mi>N</mml:mi>
