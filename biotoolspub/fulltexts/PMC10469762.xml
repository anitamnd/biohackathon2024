                      <mml:mrow>
                        <mml:mi>C</mml:mi>
                        <mml:mi>C</mml:mi>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>i</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                    </mml:msubsup>
                  </mml:mrow>
