                      <mml:mrow>
                        <mml:mi>D</mml:mi>
                        <mml:mi>C</mml:mi>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>i</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                    </mml:msubsup>
                    <mml:msubsup>
                      <mml:mi>δ</mml:mi>
                      <mml:mrow>
                        <mml:mi>j</mml:mi>
                      </mml:mrow>
