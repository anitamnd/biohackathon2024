\usepackage{amsbsy}
\usepackage{upgreek}
\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$$\begin{eqnarray*} l(\mathcal {A};\mathcal {R}) &amp;\approx &amp; \sum _{i=1}^{N} \log \frac{1}{\tilde{T}(\hat{l_i})} \sum _{j=1}^{M_i}p_F(l_{i,j})p_E(r_i|a_{i,j})\\ &amp;=&amp; \sum _{i=1}^{N} \log \frac{1}{\tilde{T}(\hat{l_i})}\\ &amp;&amp;+\, \sum _{i=1}^{N} \log \sum _{j=1}^{M_i}p_F(l_{i,j})p_E(r_i|a_{i,j}) \end{eqnarray*}$$\end{document}</tex-math></disp-formula></p>
          <p>where <inline-formula><tex-math id="M17">\documentclass[12pt]{minimal}
