                          <mml:mi>N</mml:mi>
                        </mml:mrow>
                        <mml:mrow>
                          <mml:mi>T</mml:mi>
                          <mml:mi>P</mml:mi>
                          <mml:mo>+</mml:mo>
                          <mml:mi>F</mml:mi>
                          <mml:mi>N</mml:mi>
                          <mml:mo>+</mml:mo>
                          <mml:mi>F</mml:mi>
                          <mml:mi>P</mml:mi>
