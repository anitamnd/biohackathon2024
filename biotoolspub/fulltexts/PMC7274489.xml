                      <mml:mtext> </mml:mtext>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                      <mml:mo>×</mml:mo>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                      <mml:mtext> </mml:mtext>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
