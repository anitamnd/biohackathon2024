                        <mml:mi>P</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                        <mml:mo>*</mml:mo>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>T</mml:mi>
                        <mml:mi>N</mml:mi>
                        <mml:mo>+</mml:mo>
