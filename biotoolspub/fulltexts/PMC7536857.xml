                <mml:msub>
                  <mml:mi>S</mml:mi>
                  <mml:mrow>
                    <mml:mi>k</mml:mi>
                    <mml:mo>,</mml:mo>
                    <mml:mtext> </mml:mtext>
                    <mml:mn>1</mml:mn>
                  </mml:mrow>
                </mml:msub>
                <mml:mo>,</mml:mo>
                <mml:mtext> </mml:mtext>
                <mml:msub>
                  <mml:mi>S</mml:mi>
                  <mml:mrow>
                    <mml:mi>k</mml:mi>
                    <mml:mo>,</mml:mo>
