            <td colspan="1" rowspan="1">TMEM220<inline-formula><mml:math id="MM95"><mml:mo stretchy="false">|</mml:mo></mml:math></inline-formula>388335</td>
            <td colspan="1" rowspan="1">ARHGAP20<inline-formula><mml:math id="MM96"><mml:mo stretchy="false">|</mml:mo></mml:math></inline-formula>57569</td>
            <td colspan="1" rowspan="1">SPRY2<inline-formula><mml:math id="MM97"><mml:mo stretchy="false">|</mml:mo></mml:math></inline-formula>10253</td>
          </tr>
