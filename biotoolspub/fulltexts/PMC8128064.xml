\usepackage{amsmath}
\usepackage{wasysym}
\usepackage{amsfonts}
\usepackage{amssymb}
\usepackage{amsbsy}
\usepackage{upgreek}
\usepackage{mathrsfs}
\setlength{\oddsidemargin}{-69pt}
\begin{document}
}{}$x$\end{document}</tex-math></inline-formula><italic toggle="yes">is connected to node</italic><inline-formula><tex-math id="M3">\documentclass[12pt]{minimal}
\usepackage{amsmath}
