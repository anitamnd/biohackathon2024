            <graphic xlink:href="tcp-27-73-e004.jpg" position="float" orientation="portrait"/>
          </alternatives>
          <label>(4)</label>
        </disp-formula>
      </p>
      <p>
        <disp-formula>
          <alternatives>
            <mml:math id="m5">
              <mml:mo>=</mml:mo>
              <mml:mi>  </mml:mi>
              <mml:mfrac>
                <mml:mrow>
                  <mml:mi>D</mml:mi>
                  <mml:mi>o</mml:mi>
                  <mml:mi>s</mml:mi>
                  <mml:mi>e</mml:mi>
                </mml:mrow>
                <mml:mrow>
                  <mml:mi>V</mml:mi>
                  <mml:mn>1</mml:mn>
                </mml:mrow>
              </mml:mfrac>
              <mml:msup>
                <mml:mrow>
                  <mml:mi>e</mml:mi>
                </mml:mrow>
                <mml:mrow>
                  <mml:mo>−</mml:mo>
                  <mml:mfrac>
                    <mml:mrow>
                      <mml:mi>C</mml:mi>
                      <mml:mi>L</mml:mi>
                      <mml:mn>1</mml:mn>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mi>V</mml:mi>
                      <mml:mn>1</mml:mn>
                    </mml:mrow>
