                        <mml:mi>A</mml:mi>
                        <mml:mi>S</mml:mi>
                        <mml:mi>G</mml:mi>
                        <mml:mi mathvariant="normal">s</mml:mi>
                        <mml:mtext> </mml:mtext>
                        <mml:mi>n</mml:mi>
                        <mml:mi>o</mml:mi>
                        <mml:mi>t</mml:mi>
                        <mml:mi>
</mml:mi>
                        <mml:mi>i</mml:mi>
                        <mml:mi>n</mml:mi>
                        <mml:mtext> </mml:mtext>
                        <mml:mi>p</mml:mi>
                        <mml:mi>a</mml:mi>
                        <mml:mi>t</mml:mi>
                        <mml:mi>h</mml:mi>
                        <mml:mi>w</mml:mi>
                        <mml:mi>a</mml:mi>
