				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{array}{cc}{\mathbb{E}}[\mathop{\sum }\limits_{t=1}^{T}\Delta ({{\boldsymbol{a}}}_{t})|{\bar{{\mathcal{E}}}}_{T}] &amp; \mathop{=}\limits^{({\rm{T}})}{\mathbb{E}}[\mathop{\sum }\limits_{t=1}^{T}{\mathbb{E}}[\mu ({{\boldsymbol{a}}}_{\ast })-\mu ({{\boldsymbol{a}}}_{t})|{{\mathcal{H}}}_{t-1},{{\mathcal{E}}}_{T}]]\\  &amp; ={\mathbb{E}}[\mathop{\sum }\limits_{t=1}^{T}{\mathbb{E}}[\mu ({{\boldsymbol{a}}}_{\ast })-{u}_{t}({{\boldsymbol{a}}}_{t})|{{\mathcal{H}}}_{t-1},{{\mathcal{E}}}_{T}]\,\,+\mathop{\sum }\limits_{t=1}^{T}{\mathbb{E}}[{u}_{t}({{\boldsymbol{a}}}_{t})-\mu ({{\boldsymbol{a}}}_{t})|{{\mathcal{H}}}_{t-1},{{\mathcal{E}}}_{T}]]\\  &amp; \mathop{=}\limits^{(15)}{\mathbb{E}}[\mathop{\sum }\limits_{t=1}^{T}{\mathbb{E}}[\mu ({{\boldsymbol{a}}}_{\ast })-{u}_{t}({{\boldsymbol{a}}}_{\ast })|{{\mathcal{H}}}_{t-1},{{\mathcal{E}}}_{T}]\,\,+\mathop{\sum }\limits_{t=1}^{T}{\mathbb{E}}[{u}_{t}({{\boldsymbol{a}}}_{t})-\mu ({{\boldsymbol{a}}}_{t})|{{\mathcal{H}}}_{t-1},{{\mathcal{E}}}_{T}]].\end{array}$$\end{document}</tex-math><mml:math id="M190" display="block"><mml:mtable columnalign="right left" columnspacing="0em" displaystyle="true" rowspacing="3pt"><mml:mtr><mml:mtd><mml:mrow><mml:mrow><mml:mi mathvariant="double-struck">E</mml:mi></mml:mrow></mml:mrow><mml:mrow><mml:mo>[</mml:mo><mml:munderover><mml:mrow class="MJX-TeXAtom-OP MJX-fixedlimits"><mml:mo movablelimits="false">∑</mml:mo></mml:mrow><mml:mrow><mml:mi>t</mml:mi><mml:mrow><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:mrow><mml:mrow><mml:mi>T</mml:mi></mml:mrow></mml:munderover><mml:mi mathvariant="normal">Δ</mml:mi><mml:mo>(</mml:mo><mml:msub><mml:mrow><mml:mrow><mml:mi mathvariant="bold-italic">a</mml:mi></mml:mrow></mml:mrow><mml:mrow><mml:mi>t</mml:mi></mml:mrow></mml:msub><mml:mo>)</mml:mo><mml:mrow class="MJX-TeXAtom-CLOSE"/><mml:mo>|</mml:mo><mml:mrow class="MJX-TeXAtom-OPEN"/><mml:msub><mml:mrow><mml:mrow><mml:mover><mml:mrow><mml:mrow><mml:mi class="MJX-tex-caligraphi