          </mml:msubsup>
          <mml:mspace width="0.25em"/>
          <mml:mo>=</mml:mo>
          <mml:mspace width="0.25em"/>
          <mml:msubsup>
            <mml:mi>Ï</mml:mi>
            <mml:mrow>
