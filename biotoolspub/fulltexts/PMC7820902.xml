                      </mml:mpadded>
                      <mml:mo>⁢</mml:mo>
                      <mml:mi>genes</mml:mi>
                    </mml:mrow>
                    <mml:mo stretchy="false">)</mml:mo>
                  </mml:mrow>
                </mml:mrow>
                <mml:mrow>
                  <mml:mpadded width="+2.8pt">
                    <mml:mi>Top</mml:mi>
                  </mml:mpadded>
                  <mml:mo>⁢</mml:mo>
                  <mml:mpadded width="+2.8pt">
                    <mml:mi mathvariant="normal">N</mml:mi>
                  </mml:mpadded>
                  <mml:mo>⁢</mml:mo>
                  <mml:mpadded width="+2.8pt">
                    <mml:mi>predicted</mml:mi>
                  </mml:mpadded>
                  <mml:mo>⁢</mml:mo>
                  <mml:mpadded width="+2.8pt">
                    <mml:mi>driver</mml:mi>
                  </mml:mpadded>
                  <mml:mo>⁢</mml:mo>
                  <mml:mpadded width="+2.8pt">
                    <mml:mi>genes</mml:mi>
                  </mml:mpadded>
                </mml:mrow>
              </mml:mfrac>
            </mml:mrow>
            <mml:mo>.</mml:mo>
          </mml:mrow>
        </mml:math>
      </disp-formula>
      <disp-formula id="S5.E8">
        <label>(8)</label>
        <mml:math id="M8">
          <mml:mrow>
            <mml:mrow>
              <mml:mtext>recall</mml:mtext>
              <mml:mo>=</mml:mo>
              <mml:mfrac>
                <mml:mrow>
                  <mml:mrow>
                    <mml:mo stretchy="false">(</mml:mo>
                    <mml:mrow>
                      <mml:mpadded width="+2.8pt">
                        <mml:mi>CGC</mml:mi>
                      </mml:mpadded>
                      <mml:mo>⁢</mml:mo>
                      <mml:mi>genes</mml:mi>
                    </mml:mrow>
                    <mml:mo stretchy="false">)</mml:mo>
