          <thead>
            <tr>
              <th valign="top" align="left" rowspan="1" colspan="1">dbSNP ID</th>
              <th valign="top" align="center" rowspan="1" colspan="1">Ref</th>
              <th valign="top" align="center" rowspan="1" colspan="1">Alt</th>
              <th valign="top" align="center" rowspan="1" colspan="1">Gene</th>
              <th valign="top" align="center" rowspan="1" colspan="1">Class*</th>
              <th valign="top" align="center" rowspan="1" colspan="1">Odds ratio</th>
              <th valign="top" align="center" rowspan="1" colspan="1"><italic>p</italic> (cis-eQTL)</th>
              <th valign="top" align="center" rowspan="1" colspan="1"><italic>p</italic> (Survival analysis)</th>
              <th valign="top" align="center" rowspan="1" colspan="1">Association with metastasis</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td valign="top" align="left" rowspan="1" colspan="1">rs232729</td>
              <td valign="top" align="left" rowspan="1" colspan="1">A</td>
              <td valign="top" align="left" rowspan="1" colspan="1">G</td>
