                        </mml:mrow>
                        <mml:mo>Â¯</mml:mo>
                      </mml:mover>
                    </mml:mrow>
                    <mml:mo>)</mml:mo>
                  </mml:mrow>
                </mml:mrow>
                <mml:mrow>
                  <mml:mi>T</mml:mi>
                </mml:mrow>
              </mml:msup>
              <mml:mrow>
                <mml:mo>(</mml:mo>
                <mml:mrow>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi mathvariant="bold">A</mml:mi>
