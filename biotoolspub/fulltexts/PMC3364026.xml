                        <mml:mo>[</mml:mo>
                        <mml:mrow>
                          <mml:mrow>
                            <mml:mo>(</mml:mo>
                            <mml:mrow>
                              <mml:mi mathvariant="bold">A</mml:mi>
                              <mml:mi mathvariant="bold">X</mml:mi>
                              <mml:mi>ââ</mml:mi>
                              <mml:mo>â</mml:mo>
                              <mml:mi>E</mml:mi>
                              <mml:mrow>
                                <mml:mo>(</mml:mo>
                                <mml:mrow>
                                  <mml:mi mathvariant="bold">A</mml:mi>
                                  <mml:mi mathvariant="bold">X</mml:mi>
                                </mml:mrow>
                                <mml:mo>)</mml:mo>
                              </mml:mrow>
                            </mml:mrow>
                            <mml:mo>)</mml:mo>
                          </mml:mrow>
                          <mml:msup>
                            <mml:mrow>
