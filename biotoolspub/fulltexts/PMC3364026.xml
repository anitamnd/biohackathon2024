                                  <mml:mi mathvariant="bold">Y</mml:mi>
                                  <mml:mo>â</mml:mo>
                                  <mml:mi>E</mml:mi>
                                  <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
