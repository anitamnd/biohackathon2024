              <mml:mrow>
                <mml:mn>1</mml:mn>
              </mml:mrow>
              <mml:mrow>
                <mml:mi>M</mml:mi>
              </mml:mrow>
            </mml:mfrac>
            <mml:munderover>
              <mml:mstyle displaystyle="true">
                <mml:mo>â</mml:mo>
              </mml:mstyle>
              <mml:mrow>
                <mml:mi>j</mml:mi>
                <mml:mo>=</mml:mo>
