                      <mml:mo>−</mml:mo>
                      <mml:mn>0.9800114</mml:mn>
                      <mml:mrow>
                        <mml:msup>
                          <mml:mrow>
                            <mml:mrow>
                              <mml:mtext> </mml:mtext>
                            </mml:mrow>
                            <mml:mtext> </mml:mtext>
                            <mml:mrow>
                              <mml:mtext> </mml:mtext>
                            </mml:mrow>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mrow>
                              <mml:mi mathvariant="normal">e</mml:mi>
                              <mml:mi mathvariant="normal">x</mml:mi>
                              <mml:mi mathvariant="normal">p</mml:mi>
                            </mml:mrow>
                            <mml:mo stretchy="false">(</mml:mo>
                            <mml:mi>L</mml:mi>
                            <mml:mrow>
                              <mml:msub>
                                <mml:mrow>
                                  <mml:mi>P</mml:mi>
                                </mml:mrow>
                                <mml:mrow>
                                  <mml:mn>2</mml:mn>
                                </mml:mrow>
                              </mml:msub>
                            </mml:mrow>
                            <mml:mo stretchy="false">)</mml:mo>
                          </mml:mrow>
                        </mml:msup>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                      <mml:mtext> </mml:mtext>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                    </mml:mstyle>
                  </mml:mtd>
                </mml:mtr>
              </mml:mtable>
            </mml:mstyle>
          </mml:math>
        </disp-formula>
        <p>where 0.9800114 is the baseline survival estimate for no change in residence and the linear predictor (LP<sub>2</sub>) is equal to</p>
        <disp-formula id="UN0004">
          <mml:math id="M04" overflow="scroll">
            <mml:mstyle displaystyle="true">
