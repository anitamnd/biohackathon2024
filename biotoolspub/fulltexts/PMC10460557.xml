                  <mml:mtd>
                    <mml:mstyle displaystyle="true">
                      <mml:mn>1</mml:mn>
                      <mml:mo>−</mml:mo>
                      <mml:mrow>
                        <mml:msup>
                          <mml:mrow>
                            <mml:mn>0.9909</mml:mn>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:mrow>
                              <mml:mi mathvariant="normal">e</mml:mi>
                              <mml:mi mathvariant="normal">x</mml:mi>
                              <mml:mi mathvariant="normal">p</mml:mi>
                            </mml:mrow>
                            <mml:mo stretchy="false">(</mml:mo>
                            <mml:mrow>
                              <mml:mi mathvariant="normal">L</mml:mi>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:msub>
                                <mml:mrow>
                                  <mml:mrow>
                                    <mml:mi mathvariant="normal">P</mml:mi>
                                  </mml:mrow>
                                </mml:mrow>
                                <mml:mrow>
                                  <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                  </mml:mrow>
                                </mml:mrow>
                              </mml:msub>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mo stretchy="false">)</mml:mo>
                            </mml:mrow>
                          </mml:mrow>
                        </mml:msup>
                      </mml:mrow>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                      <mml:mtext> </mml:mtext>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                    </mml:mstyle>
                  </mml:mtd>
                </mml:mtr>
              </mml:mtable>
            </mml:mstyle>
          </mml:math>
        </disp-formula>
