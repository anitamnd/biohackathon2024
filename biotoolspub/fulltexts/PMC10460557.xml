                    <mml:mtext> </mml:mtext>
                    <mml:mo>+</mml:mo>
                    <mml:mtext> </mml:mtext>
                    <mml:mo stretchy="false">(</mml:mo>
                    <mml:mrow>
                      <mml:msub>
                        <mml:mrow>
                          <mml:mi>β</mml:mi>
                        </mml:mrow>
