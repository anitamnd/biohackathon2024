                          <mml:mo>+</mml:mo>
                          <mml:mrow>
                            <mml:mtext> </mml:mtext>
                          </mml:mrow>
                          <mml:mtext> </mml:mtext>
                          <mml:mrow>
                            <mml:mtext> </mml:mtext>
                            <mml:mi mathvariant="normal">e</mml:mi>
                            <mml:mi mathvariant="normal">x</mml:mi>
                          </mml:mrow>
                          <mml:mrow>
                            <mml:msup>
                              <mml:mrow>
                                <mml:mrow>
                                  <mml:mi mathvariant="normal">p</mml:mi>
                                </mml:mrow>
                              </mml:mrow>
                              <mml:mrow>
                                <mml:mo>−</mml:mo>
                                <mml:mrow>
                                  <mml:mi mathvariant="normal">L</mml:mi>
                                </mml:mrow>
                                <mml:mrow>
                                  <mml:msub>
                                    <mml:mrow>
                                      <mml:mrow>
                                        <mml:mi mathvariant="normal">P</mml:mi>
                                      </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                      <mml:mn>3</mml:mn>
                                    </mml:mrow>
                                  </mml:msub>
                                </mml:mrow>
                              </mml:mrow>
                            </mml:msup>
                          </mml:mrow>
                        </mml:mrow>
                      </mml:mfrac>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                      <mml:mtext> </mml:mtext>
                      <mml:mrow>
                        <mml:mtext> </mml:mtext>
                      </mml:mrow>
                    </mml:mstyle>
                  </mml:mtd>
                </mml:mtr>
              </mml:mtable>
            </mml:mstyle>
          </mml:math>
        </disp-formula>
        <p>where the linear predictor (LP<sub>3</sub>) is equal to</p>
        <disp-formula id="UN0006">
          <mml:math id="M06" overflow="scroll">
            <mml:mstyle displaystyle="true">
              <mml:mtable columnalign="left" columnspacing="1">
                <mml:mtr>
                  <mml:mtd/>
                  <mml:mtd>
                    <mml:mo>−</mml:mo>
