                <mml:mtr>
                  <mml:mtd>
                    <mml:mstyle class="text">
                      <mml:mtext class="textsf" mathvariant="sans-serif">PPV</mml:mtext>
                    </mml:mstyle>
                    <mml:mo class="MathClass-rel">=</mml:mo>
                    <mml:mfrac>
                      <mml:mrow>
                        <mml:mstyle class="text">
