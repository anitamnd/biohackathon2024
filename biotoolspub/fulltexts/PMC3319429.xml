                                </mml:mstyle>
                                <mml:mo class="MathClass-bin">+</mml:mo>
                                <mml:mstyle class="text">
                                  <mml:mtext class="textsf" mathvariant="sans-serif">FP</mml:mtext>
                                </mml:mstyle>
                              </mml:mrow>
                              <mml:mo class="MathClass-close">)</mml:mo>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mo class="MathClass-open">(</mml:mo>
                              <mml:mrow>
                                <mml:mstyle class="text">
                                  <mml:mtext class="textsf" mathvariant="sans-serif">TP</mml:mtext>
                                </mml:mstyle>
                                <mml:mo class="MathClass-bin">+</mml:mo>
                                <mml:mstyle class="text">
                                  <mml:mtext class="textsf" mathvariant="sans-serif">FN</mml:mtext>
                                </mml:mstyle>
                              </mml:mrow>
                              <mml:mo class="MathClass-close">)</mml:mo>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mo class="MathClass-open">(</mml:mo>
                              <mml:mrow>
                                <mml:mstyle class="text">
                                  <mml:mtext class="textsf" mathvariant="sans-serif">TN</mml:mtext>
                                </mml:mstyle>
                                <mml:mo class="MathClass-bin">+</mml:mo>
                                <mml:mstyle class="text">
                                  <mml:mtext class="textsf" mathvariant="sans-serif">FP</mml:mtext>
                                </mml:mstyle>
                              </mml:mrow>
                              <mml:mo class="MathClass-close">)</mml:mo>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mo class="MathClass-open">(</mml:mo>
                              <mml:mrow>
                                <mml:mstyle class="text">
                                  <mml:mtext class="textsf" mathvariant="sans-serif">TN</mml:mtext>
