              <col span="1"/>
            </colgroup>
            <thead>
              <tr>
                <th rowspan="1" colspan="1">Parameter</th>
                <th rowspan="1" colspan="1">Wikipedia</th>
                <th rowspan="1" colspan="1">PubMed</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td rowspan="1" colspan="1">TP</td>
                <td rowspan="1" colspan="1">(31.11%) 2,075</td>
