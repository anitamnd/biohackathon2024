                      <mml:mo stretchy="false">)</mml:mo>
                      <mml:mtext mathvariant="italic">HW</mml:mtext>
                      <mml:mo stretchy="false">)</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext mathvariant="italic">kn</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>w</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext mathvariant="italic">kn</mml:mtext>
                    </mml:mrow>
