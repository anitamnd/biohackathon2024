                      <mml:mo stretchy="false">)</mml:mo>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext mathvariant="italic">dk</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mi>h</mml:mi>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mtext mathvariant="italic">dk</mml:mtext>
                    </mml:mrow>
                  </mml:msub>
                </mml:mtd>
              </mml:mtr>
              <mml:mtr>
                <mml:mtd>
                  <mml:mo>+</mml:mo>
                  <mml:mn>2</mml:mn>
                  <mml:msub>
                    <mml:mrow>
                      <mml:mo stretchy="false">(</mml:mo>
                      <mml:mtext mathvariant="italic">diag</mml:mtext>
                      <mml:mo stretchy="false">(</mml:mo>
