          <mml:math id="M35" name="1471-2105-14-107-i35" overflow="scroll">
            <mml:mtable columnalign="left">
              <mml:mtr>
                <mml:mtd>
                  <mml:mo>−</mml:mo>
                  <mml:mn>2</mml:mn>
