        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">2</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let K = Number of Neighbours</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">3</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let D = Dunn Index Value</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">4</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let H = Number of Highly Variable Genes</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">5</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let A = Clustering Algorithm</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">6</td>
          <td valign="top" align="left" rowspan="1" colspan="1">for K = 1:50</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1"/>
          <td valign="top" align="left" rowspan="1" colspan="1"> P = <italic toggle="yes">denoisePCA</italic>(X, H)</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1"/>
          <td valign="top" align="left" rowspan="1" colspan="1"> S = buildSNNGraph(P, K)</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1"/>
          <td valign="top" align="left" rowspan="1" colspan="1"> C = clustering(S, A)</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1"/>
          <td valign="top" align="left" rowspan="1" colspan="1"> D = DunnIndex(P, C)</td>
        </tr>
