  <table-wrap position="float" id="t3-tjb-47-06-413">
    <label>Algorithm 1</label>
    <caption>
      <p>Preparation of Clustering Evaluation Dataset</p>
    </caption>
    <table frame="box" rules="none">
      <tbody>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">1</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let X = scRNA-Seq Datasets</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">2</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let K = Number of Neighbours</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">3</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let P = Principal Component Analysis Result</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">4</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let PC = Number of Principal Components Used</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">5</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let H = Number of Highly Variable Genes</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">6</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let A = Algorithm</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">7</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let S = SNN Graph</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">8</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let C = Clustering Results</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">9</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let L = Cell Labels</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">10</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let ARI = Adjusted Rand Index</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">11</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let E = Experiment Type (UMI, Spike)</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">12</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let XN = Number of Cells in scRNA-Seq Dataset</td>
        </tr>
        <tr>
          <td valign="top" align="left" rowspan="1" colspan="1">13</td>
          <td valign="top" align="left" rowspan="1" colspan="1">let VE = Percentage of Variance Explained with PC</td>
        </tr>
