                            </mml:mrow>
                          </mml:msub>
                        </mml:mrow>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                      <mml:mo>-</mml:mo>
                      <mml:mi>a</mml:mi>
                      <mml:mi>v</mml:mi>
                      <mml:mi>e</mml:mi>
                      <mml:mrow>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mrow>
                          <mml:msubsup>
                            <mml:mrow>
                              <mml:mi>P</mml:mi>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mi>g</mml:mi>
                            </mml:mrow>
                            <mml:mrow>
                              <mml:mn>0</mml:mn>
                            </mml:mrow>
                          </mml:msubsup>
                          <mml:mrow>
                            <mml:mo stretchy="false">(</mml:mo>
                            <mml:mrow>
                              <mml:msub>
                                <mml:mrow>
                                  <mml:mi>D</mml:mi>
                                </mml:mrow>
                                <mml:mrow>
                                  <mml:mi>i</mml:mi>
                                </mml:mrow>
                              </mml:msub>
                            </mml:mrow>
                            <mml:mo stretchy="false">)</mml:mo>
                          </mml:mrow>
                        </mml:mrow>
                        <mml:mo stretchy="false">)</mml:mo>
                      </mml:mrow>
                    </mml:mrow>
                    <mml:mrow>
                      <mml:mi>S</mml:mi>
                      <mml:mi>D</mml:mi>
                      <mml:mrow>
                        <mml:mo stretchy="false">{</mml:mo>
                        <mml:mrow>
                          <mml:mi>a</mml:mi>
                          <mml:mi>v</mml:mi>
                          <mml:mi>e</mml:mi>
                          <mml:mrow>
                            <mml:mo stretchy="true">(</mml:mo>
                            <mml:mrow>
                              <mml:msubsup>
                                <mml:mrow>
                                  <mml:mi>P</mml:mi>
                                </mml:mrow>
                                <mml:mrow>
                                  <mml:mi>g</mml:mi>
                                </mml:mrow>
                                <mml:mrow>
                                  <mml:mn>0</mml:mn>
                                </mml:mrow>
                              </mml:msubsup>
                              <mml:mrow>
                                <mml:mo stretchy="false">(</mml:mo>
                                <mml:mrow>
                                  <mml:msub>
                                    <mml:mrow>
                                      <mml:mi>D</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                      <mml:mi>i</mml:mi>
                                    </mml:mrow>
                                  </mml:msub>
                                </mml:mrow>
                                <mml:mo stretchy="false">)</mml:mo>
