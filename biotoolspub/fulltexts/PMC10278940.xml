                      </mml:mstyle>
                      <mml:mo stretchy="false">)</mml:mo>
                    </mml:mrow>
                  </mml:msub>
                </mml:mrow>
                <mml:mo stretchy="false">[</mml:mo>
                <mml:mi>max</mml:mi>
                <mml:mo stretchy="false">(</mml:mo>
