            <mml:mn>1</mml:mn>
            <mml:mo>−</mml:mo>
            <mml:msub>
              <mml:mover accent="true">
                <mml:mi>y</mml:mi>
                <mml:mo>^</mml:mo>
              </mml:mover>
              <mml:mi>k</mml:mi>
            </mml:msub>
            <mml:mo stretchy="false">)</mml:mo>
            <mml:mo stretchy="false">]</mml:mo>
