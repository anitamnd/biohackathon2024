                        </mml:mrow>
                        <mml:mrow>
                          <mml:mi>i</mml:mi>
                          <mml:mi>n</mml:mi>
                          <mml:mi>t</mml:mi>
                          <mml:mi>r</mml:mi>
                          <mml:mi>a</mml:mi>
                          <mml:mo>-</mml:mo>
