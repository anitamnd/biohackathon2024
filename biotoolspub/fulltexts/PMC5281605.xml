                          <mml:mi>t</mml:mi>
                          <mml:mi>r</mml:mi>
                          <mml:mi>a</mml:mi>
                          <mml:mo>-</mml:mo>
                          <mml:mi>a</mml:mi>
                          <mml:mi>x</mml:mi>
                          <mml:mi>o</mml:mi>
                          <mml:mi>n</mml:mi>
                          <mml:mi>a</mml:mi>
                          <mml:mi>l</mml:mi>
                        </mml:mrow>
                      </mml:msub>
                      <mml:mo>+</mml:mo>
                      <mml:msub>
                        <mml:mrow>
                          <mml:mi>A</mml:mi>
                        </mml:mrow>
                        <mml:mrow>
                          <mml:mi>e</mml:mi>
                          <mml:mi>x</mml:mi>
                          <mml:mi>t</mml:mi>
                          <mml:mi>r</mml:mi>
                          <mml:mi>a</mml:mi>
                          <mml:mo>-</mml:mo>
