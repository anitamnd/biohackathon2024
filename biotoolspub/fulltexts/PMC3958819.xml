                                <mml:mi>f</mml:mi>
                              </mml:mrow>
                              <mml:mn>1</mml:mn>
                              <mml:mrow>
                                <mml:mo stretchy="false">(</mml:mo>
                                <mml:mn>1</mml:mn>
                                <mml:mo stretchy="false">)</mml:mo>
