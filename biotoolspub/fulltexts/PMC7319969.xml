				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{array}{l}P(\mathop{{\bf{C}}}\limits_{\sim}|\mathop{{\bf{B}}\hspace{0.0005pt}}\limits_{\sim})=\int p({\mathop{{\bf{C}}}\limits_{\sim}}_{1}|\mathop{{\bf{B}}\hspace{0.0005pt}}\limits_{\sim},\mathop{{\bf{W}}}\limits_{\sim})\times p({\mathop{{\bf{C}}}\limits_{\sim}}_{2}|{\mathop{{\bf{C}}}\limits_{\sim}}_{1},\mathop{{\bf{B}}\hspace{0.0005pt}}\limits_{\sim},\mathop{{\bf{W}}}\limits_{\sim})\times \cdots \times p({\mathop{{\bf{C}}}\limits_{\sim}}_{M}|{\mathop{{\bf{C}}}\limits_{\sim}}_{M-1},\mathop{{\bf{B}}\hspace{0.0005pt}}\limits_{\sim},\mathop{{\bf{W}}}\limits_{\sim})\times P(\mathop{{\bf{W}}}\limits_{\sim}|\mathop{{\bf{B}}}\limits_{\sim})d\mathop{{\bf{W}}}\limits_{\sim}\\ \,=\,\{\mathop{\prod }\limits_{k=1}^{K}\frac{\varGamma ({C}_{1k}+{w}_{0k})}{\varGamma ({w}_{0k})}\}\times \mathop{\prod }\limits_{k\text{'}=1}^{K}\{\frac{1}{\varGamma (1+{n}_{k\text{'}})}\mathop{\prod }\limits_{\begin{array}{c}k=1\\ k\ne k\text{'}\end{array}}^{K}\frac{\varGamma ({n}_{k\text{'}k}+\frac{{w}_{0k}}{1-{w}_{0k\text{'}}})}{\varGamma (\frac{{w}_{0k}}{1-{w}_{0k\text{'}}})}\}\end{array}$$\end{document}</tex-math><mml:math id="M102" display="block"><mml:mtable columnalign="left"><mml:mtr><mml:mtd columnalign="left"><mml:mrow><mml:mi>P</mml:mi><mml:mrow><mml:mo stretchy="true">(</mml:mo><mml:mrow><mml:munder><mml:mi mathvariant="bold">C</mml:mi><mml:mo mathvariant="bold">˜</mml:mo></mml:munder><mml:mo>|</mml:mo><mml:munder><mml:mrow><mml:mi mathvariant="bold">B</mml:mi><mml:mspace width="0.0005pt"/></mml:mrow><mml:mo mathvariant="bold" stretchy="true">˜</mml:mo></mml:munder></mml:mrow><mml:mo stretchy="true">)</mml:mo></mml:mrow><mml:mo>=</mml:mo><mml:mo>∫</mml:mo><mml:mrow><mml:mi>p</mml:mi><mml:mrow><mml:mo stretchy="true">(</mml:mo><mml:mrow><mml:msub><mml:mrow><mml:munder><mml:mi mathvariant="bold">C</mml:mi><mml:mo mathvariant="bold">˜</mml:mo></mml:munder></mml:mrow><mml:mrow><mml:mn>1</mml:mn></mml:mrow></mml:msub><mml:mo>|</mml:mo><mml:munder><mml:mrow><mml:mi ma